          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(RESOURCEDEF.RESOURCEID) as RSRS_ID\n, TRIM(RESOURCEDEF.FACTORYID) as FCTRY_ID\n, TRIM(RESOURCEDEF.LOCATIONID) as LOC_ID\n, CAST(NULL AS STRING) as SPEC_ID\n, CAST(NULL AS STRING) as ALT_ASSET_ID\n, CAST(NULL AS STRING) as ASSET_SITE_NM\n, CAST(NULL AS STRING) as ASYNC_TMOUT_TXT\n, CAST(NULL AS BOOLEAN) as CALIBRATION_ASSET_IND\n, CAST(NULL AS STRING) as CIO_CHNL_ADPT_ID\n, INT(RESOURCEDEF.CHANGECOUNT) as CHG_CNT\n, TRIM(RESOURCEDEF.CHANGESTATUSID) as CHG_STS_ID\n, TRIM(RESOURCEDEF.CLEARANCELEVEL) as CLRNC_LVL_CD\n, CAST(NULL AS INT) as CNTNR_CAPY_CNT\n, CAST(NULL AS STRING) as CNTNR_STS_CD\n, TRIM(RESOURCEDEF.DECIMALPRECISION) as DEC_PRCN_VAL\n, TRIM(RESOURCEDEF.DECIMALSCALE) as DEC_SCALE_VAL\n, CAST(NULL AS STRING) as DFLT_EXPRSSN_TXT\n, CAST(NULL AS STRING) as DFLT_EXPRSSN_TYPE_CD\n, CAST(NULL AS BOOLEAN) as EXTRNL_STS_IND\n, TRIM(RESOURCEDEF.FULLYQUALIFIEDNAME) as FULL_QUAL_NM\n, TRIM(RESOURCEDEF.ICONID) as ICON_ID\n, CAST(NULL AS BOOLEAN) as INCL_IN_OEE_IND\n, CAST(NULL AS BOOLEAN) as INCL_RAW_DATA_IND\n, CAST(RESOURCEDEF.ISFROZEN as BOOLEAN ) as IS_FRZN_IND\n, CAST(NULL AS BOOLEAN) as IS_RESL_DNT_QRY_IND\n, CAST(NULL AS BOOLEAN) as IS_VLD_QUE_IND\n, CAST(NULL AS STRING) as LINE_ID\n, CAST(NULL AS BOOLEAN) as LOGGING_IND\n, TRIM(RESOURCEDEF.MAINTENANCECLASSID) as MAINT_CLS_ID\n, CAST(NULL AS STRING) as MATL_QUE_BOM_ID\n, CAST(NULL AS STRING) as MAXIMO_DSTN_ID\n, CAST(NULL AS STRING) as MAXIMO_GL_ACCT_CD\n, CAST(NULL AS STRING) as MAXIMO_RSRS_NM\n, CAST(NULL AS STRING) as MAX_CAPY_VAL\n, CAST(NULL AS STRING) as MIN_CAPY_VAL\n, TRIM(RESOURCEDEF.CDOTYPEID) as OBJ_TYPE_ID\n, CAST(NULL AS STRING) as OEE_SETS_ID\n, CAST(NULL AS STRING) as OPC_CHNL_ID\n, CAST(NULL AS STRING) as OPC_MACH_ID\n, CAST(NULL AS STRING) as OPC_SERVR_ID\n, TRIM(RESOURCEDEF.PARENTRESOURCEID) as PARNT_RSRS_ID\n, CAST(NULL AS STRING) as PC_ID\n, CAST(NULL AS STRING) as PORT_ID\n, TRIM(RESOURCEDEF.PRINTQUEUEID) as PRT_QUE_ID\n, TRIM(RESOURCEDEF.PRODUCTIONSTATUSID) as PRDTN_STS_ID\n, CAST(NULL AS STRING) as PRMT_TXT\n, CAST(NULL AS STRING) as READABL_VAL\n, CAST(NULL AS DECIMAL(18,4)) as RSRS_CAPY_QTY\n, TRIM(RESOURCEDEF.DESCRIPTION) as RSRS_DESC\n, TRIM(RESOURCEDEF.RESOURCEFAMILYID) as RSRS_FMLY_ID\n, TRIM(RESOURCEDEF.RESOURCENAME) as RSRS_NM\n, CAST(NULL AS STRING) as RSRS_NUM\n, CAST(NULL AS STRING) as RSRS_PART_ID\n, TRIM(RESOURCEDEF.RESOURCESTATUSMODELID) as RSRS_STS_MDL_ID\n, TRIM(RESOURCEDEF.RESOURCETYPEID) as RSRS_TYPE_ID\n, CAST(NULL AS STRING) as SGNTR_REQ_ID\n, CAST(NULL AS INT) as TIME_TO_LIVE_SECS_CNT\n, CAST(NULL AS INT) as TRNG_RCNT_EXP_DAYS_CNT\n, CAST(NULL AS STRING) as TRAIN_RCNT_ID\n, TRIM(RESOURCEDEF.TRAININGREQGROUPID) as TRAIN_REQ_GRP_ID\n, TRIM(RESOURCEDEF.UOMID) as UOM_ID\n, TRIM(RESOURCEDEF.VENDORID) as VEND_ID\n, TRIM(RESOURCEDEF.VENDORMODEL) as VEND_MDL_NUM\n, TRIM(RESOURCEDEF.VENDORSERIALNUMBER) as VEND_SER_NUM\n, TRIM(RESOURCEDEF.WEBSERVICETRANSPORTID) as WEB_SRVC_TRNSP_ID\n, TRIM(RESOURCEDEF.WIPMSGDEFMGRID) as WIP_MSG_DEF_MGR_ID\n, TRIM(RESOURCEDEF.WORKINGRANGEMAX) as WRK_RNG_MAX_VAL\n, TRIM(RESOURCEDEF.WORKINGRANGEMIN) as WRK_RNG_MIN_VAL\n, CAST(NULL AS STRING) as ETH_PRT_QUE_ID\n, CAST(NULL AS STRING) as ETH_RSRS_TYPE_ID\n, TRIM(RESOURCEDEF.LASTREVTXNID) as NOTES_TXT\n, TRIM(RESOURCEDEF.NOTES) as LAST_REVERSABLE_TXN_ID\nFROM  {Config.sourceDatabase}.RESOURCEDEF as RESOURCEDEF WHERE RESOURCEDEF._deleted_ =  'F'\n  \n \n\"\"\")",
