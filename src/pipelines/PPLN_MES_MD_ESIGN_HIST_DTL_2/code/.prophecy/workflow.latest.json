          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(ESIGHISTORYDETAIL.ESIGHISTORYDETAILID) as ESIGN_HIST_DTL_ID\n, TRIM(ESIGHISTORYDETAIL.ESIGHISTORYSUMMARYID) as ESIGN_HIST_SUM_ID\n, INT(ESIGHISTORYDETAIL.CHANGECOUNT) as CHG_CNT\n, TRIM(ESIGHISTORYDETAIL.COMMENTS) as COMT_TXT\n, TRIM(ESIGHISTORYDETAIL.COSIGNREASONID) as COSIGN_RSN_ID\n, TRIM(ESIGHISTORYDETAIL.COSIGNERFULLNAME) as COSIGN_FULL_NM\n, TRIM(ESIGHISTORYDETAIL.COSIGNERID) as COSIGN_ID\n, CAST(NULL AS STRING) as EXPT_IMPT_KEY_VAL\n, TRIM(ESIGHISTORYDETAIL.HISTORYID) as HIST_ID\n, TRIM(ESIGHISTORYDETAIL.CDOTYPEID) as OBJ_TYPE_ID\n, TRIM(ESIGHISTORYDETAIL.SIGNERFULLNAME) as SIGN_FULL_NM\n, TRIM(ESIGHISTORYDETAIL.SIGNERID) as SIGN_ID\n, TRIM(ESIGHISTORYDETAIL.TXNID) as TXN_ID\nFROM  {Config.sourceDatabase}.ESIGHISTORYDETAIL as ESIGHISTORYDETAIL WHERE ESIGHISTORYDETAIL._deleted_ =  'F'\n  \n \n\"\"\")",
