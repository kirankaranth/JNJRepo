          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n     select SRC_SYS_CD,MATL_NUM,PLNT_CD,BOM_USG_CD,BOM_NUM,ALT_BOM_NUM,CRT_DTTM,CHG_DTTM,FROM_LOT_SIZE_QTY,TO_LOT_SIZE_QTY from\n  (select *,\nROW_NUMBER() over (partition by SRC_SYS_CD,MATL_NUM,PLNT_CD,BOM_USG_CD,BOM_NUM,ALT_BOM_NUM order by MATL_NUM) as RNK\nfrom (select\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\nf3002.ixkitl AS MATL_NUM,\nf3002.ixmmcu AS PLNT_CD,\nf3002.ixtbm  AS BOM_USG_CD,\nconcat(f3002.ixkit,';',f3002.ixmmcu,';' ,f3002.ixtbm,';',f3002.ixbqty,';',f3002.ixcoby,';',f3002.ixcpnt,';',f3002.ixsbnt) AS BOM_NUM,\n\\\"01\\\" AS ALT_BOM_NUM,\nCASE\n  WHEN LOWER(TRIM(ixefff)) = 'null' OR TRIM(ixefff) = '' OR TRIM(ixefff) = '0' THEN NULL\n  ELSE to_timestamp(substr(CAST(date_add(concat(substr(CAST(CAST(TRIM(ixefff) AS INT) + 1900000 AS STRING),1,4),'-01-01'), CAST(substr(CAST(CAST(TRIM(ixefff) AS INT) + 1900000 AS string),5) AS INT )-1) AS STRING), 1, 10),'yyyy-MM-dd')\n  END AS CRT_DTTM ,\nCASE\n  WHEN LOWER(TRIM(ixupmj)) = 'null' OR TRIM(ixupmj) = '' OR TRIM(ixupmj) = '0' THEN CAST(NULL AS TIMESTAMP)\n  ELSE to_timestamp(concat(substr(CAST(date_add(concat(substr(CAST(CAST(TRIM(f3002.ixupmj) AS INT) + 1900000 as string),1,4),'-01-01'), CAST(substr(CAST(CAST(TRIM(f3002.ixupmj) AS INT) + 1900000 AS string),5) AS INT )-1) AS string), 1, 10),' ', lpad(TRIM(ixtday), 6, '0')), \\\"yyyy-MM-dd HHmmss\\\")\n  END AS CHG_DTTM,\nNULL AS FROM_LOT_SIZE_QTY,\nNULL AS TO_LOT_SIZE_QTY\nFROM {Config.sourceDatabase}.f3002 f3002\nWHERE f3002._deleted_ = 'F' ))\nwhere RNK =1  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
