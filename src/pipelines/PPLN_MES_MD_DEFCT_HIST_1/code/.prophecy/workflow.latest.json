          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(DEFECTHISTORY.DefectHistoryId) as DEFCT_HIST_ID\n, TRIM(DEFECTHISTORY.HistoryMainlineId) as HIST_MNLINE_ID\n, INT(DEFECTHISTORY.ChangeCount) as CHG_CNT\n, TRIM(DEFECTHISTORY.ChargeToStepId) as CHRG_TO_STEP_ID\n, TRIM(DEFECTHISTORY.ContainerLevelInspectedId) as CNTNR_LVL_INSPT_ID\n, INT(DEFECTHISTORY.ContainersInspected) as CNTNRS_INSPT_CNT\n, TRIM(DEFECTHISTORY.HistoryId) as HIST_ID\n, CAST(DEFECTHISTORY.QtyInspected AS DECIMAL(18,4)) as INSPT_QTY\n, CAST(DEFECTHISTORY.Qty2Inspected AS DECIMAL(18,4)) as INSPT_2_QTY\n, TRIM(DEFECTHISTORY.CDOTypeId) as OBJ_TYPE_ID\n, TRIM(DEFECTHISTORY.ResourceId) as RSRS_ID\n, TRIM(DEFECTHISTORY.TxnId) as TXN_ID\nFROM {Config.sourceDatabase}.DEFECTHISTORY as DEFECTHISTORY\nWHERE DEFECTHISTORY._deleted_ =  'F'  \n \n\"\"\")",
