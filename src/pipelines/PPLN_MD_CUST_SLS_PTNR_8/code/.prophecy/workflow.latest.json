          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'  AS SRC_SYS_CD,\n  TRIM(knvp.kunnr) AS CUST_NUM,\n  TRIM(knvp.vtweg) AS DSTR_CHNL_CD,\n  TRIM(knvp.spart) AS DIV_CD,\n  TRIM(knvp.parza) AS PTNR_CNTR_NBR,\n  TRIM(knvp.parvw) AS PTNR_FUNC_CD,\n  TRIM(knvp.vkorg) AS SLORG_NUM,\n  TRIM(knvp.lifnr) AS SUP_NUM,\n  TRIM(knvp.defpa) AS DFLT_PTNR_IND,\n  TRIM(knvp.kunn2) AS PTNR_NUM,\n  TRIM(tpart.vtext) AS NM_BUSN_PTNR_FUNC,\n  TRIM(tpart.parvw) AS PTNR_FUNC_TEXT,\n  TRIM(tvkot.vtext) AS NM_SLORG,\n  TRIM(tvkot.vkorg) AS SLORG_TEXT,\n  TRIM(tvtwt.vtext) AS NM_DSTN,\n  TRIM(tvtwt.vtweg) AS DSTN_CHNL_TEXT,\n  TRIM(tspat.vtext) AS NM_SLS,\n  TRIM(tspat.spart) AS DIV_TEXT\nFROM\n  {Config.sourceDatabase}.knvp knvp\n  LEFT JOIN {Config.sourceDatabase}.tpart tpart ON knvp.parvw = tpart.parvw\n  AND tpart._deleted_ = 'F'\n  AND tpart.spras = \\\"E\\\"\n  AND tpart.mandt = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')\n  LEFT JOIN {Config.sourceDatabase}.tvkot tvkot ON knvp.vkorg=tvkot.vkorg\n  AND tvkot._deleted_ = 'F'\n  AND tvkot.spras = \\\"E\\\"\n  AND tvkot.mandt = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')\n  LEFT JOIN {Config.sourceDatabase}.tvtwt tvtwt ON knvp.vtweg = tvtwt.vtweg\n  AND tvtwt._deleted_ = 'F'\n  AND tvtwt.spras = \\\"E\\\"\n  AND tvtwt.mandt = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')\n  LEFT JOIN {Config.sourceDatabase}.tspat tspat ON knvp.spart = tspat.spart\n  AND tspat._deleted_ = 'F'\n  AND tspat.spras = \\\"E\\\"\n  AND tspat.mandt = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')\n  WHERE knvp._deleted_ = 'F'\n  AND knvp.mandt = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
