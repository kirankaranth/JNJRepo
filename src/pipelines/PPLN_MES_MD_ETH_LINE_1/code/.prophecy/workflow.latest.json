          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(ETHLINE.ETHLINEID) as ETH_LINE_ID\n, INT(ETHLINE.ASSOCIATEDPACKAGES) as ASSCTD_PKGS_CNT\n, INT(ETHLINE.CHANGECOUNT) as CHG_CNT\n, TRIM(ETHLINE.CHANGEHISTORYID) as CHG_HIST_ID\n, TRIM(ETHLINE.DESCRIPTION) as ETH_LINE_DESC\n, TRIM(ETHLINE.ETHLINENAME) as ETH_LINE_NM\n, TRIM(ETHLINE.ICONID) as ICON_ID\n, CAST(ETHLINE.INSTANCELOCKED as BOOLEAN ) as INST_LOCK_IND\n, CAST(ETHLINE.ISFROZEN as BOOLEAN ) as IS_FRZN_IND\n, TRIM(ETHLINE.NOTES) as NOTES_TXT\n, TRIM(ETHLINE.CDOTypeId) as OBJ_TYPE_ID\nFROM {Config.sourceDatabase}.ETHLINE as ETHLINE\nWHERE ETHLINE._deleted_ =  'F'\n  \n \n\"\"\")",
