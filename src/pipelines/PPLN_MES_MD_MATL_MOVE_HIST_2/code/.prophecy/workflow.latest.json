          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n   '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(MoveHistory.MOVEHISTORYID) as MATL_MOVE_HIST_ID\n, TRIM(MoveHistory.PRODUCTID) as PROD_ID\n, TRIM(MoveHistory.TOFACTORYID) as TO_FCTRY_ID\n, TRIM(MoveHistory.TOLOCATIONID) as TO_LOC_ID\n, TRIM(MoveHistory.TORESOURCEID) as TO_RSRS_ID\n, TRIM(MoveHistory.TOSPECID) as TO_SPEC_ID\n, INT(MoveHistory.CHANGECOUNT) as CHG_CNT\n, INT(MoveHistory.CHILDCOUNT) as CHILD_CNT\n, CAST(TRIM(MoveHistory.CLOSEFLAG) AS BOOLEAN) as CLSE_IND\n, TRIM(MoveHistory.CONTAINERSTATUS) as CNTNR_STS_CD\n, INT(MoveHistory.CUMULATIVEREWORKSTEPCOUNT) as CUM_RWRK_STEP_CNT\n, TRIM(MoveHistory.CYCLETIME) as CYC_TIME_VAL\n, TRIM(MoveHistory.ELAPSEDTIME) as ELAP_TIME_VAL\n, TRIM(MoveHistory.ENDREWORKSTEPID) as END_RWRK_STEP_ID\n, NULL as EXPT_IMPT_KEY_VAL\n, NULL as GRADE_CHG_RSN_CD\n, TRIM(MoveHistory.HISTORYID) as HIST_ID\n, TRIM(MoveHistory.HISTORYMAINLINEID) as HIST_MNLINE_ID\n, CAST(NULL AS BOOLEAN) as ICON_STS_FLD_IND\n, NULL as LOT_RSN_ID\n, CAST(TRIM(MoveHistory.MOVEALLQTY) AS BOOLEAN) as MOVE_ALL_QTY_IND\n, CAST(MoveHistory.QTY AS DECIMAL(18, 4)) as MOVE_QTY\n, CAST(MoveHistory.QTY2 AS DECIMAL(18, 4)) as MOVE_2_QTY\n, NULL as NEW_GRADE_VAL\n, TRIM(MoveHistory.CDOTYPEID) as OBJ_TYPE_ID\n, NULL as ORIG_GRADE_VAL\n, TRIM(MoveHistory.OWNERID) as OWN_ID\n, TRIM(MoveHistory.PATHID) as PATH_ID\n, NULL as RECON_CNT\n, TRIM(MoveHistory.REENTRYSTEPID) as REENT_STEP_ID\n, TRIM(MoveHistory.REMOTECONTAINERSTATUS) as RMTE_CNTNR_STS_CD\n, TRIM(MoveHistory.REWORKREASONID) as RWRK_RSN_ID\n, CAST(TRIM(MoveHistory.RUN) AS BOOLEAN) as RUN_IND\n, TRIM(MoveHistory.SETUPID) as SETUP_ID\n, TRIM(MoveHistory.SHIPMENTDESTINATIONID) as SHIP_DEST_ID\n, TRIM(MoveHistory.STEPID) as STEP_ID\n, NULL as STERILIZATION_LINE_ID\n, TRIM(MoveHistory.TOCUSTOMERID) as TO_CUST_ID\n, TRIM(MoveHistory.TOSTEPID) as TO_STEP_ID\n, TRIM(MoveHistory.TOSTEPUSAGE) as TO_STEP_USG_ID\n, TRIM(MoveHistory.TXNID) as TXN_ID\n, INT(MoveHistory.UNITCOUNT) as UNIT_CNT\n, TRIM(MoveHistory.UOMID) as UOM_ID\n, TRIM(MoveHistory.UOM2ID) as UOM_2_ID\n, TRIM(MoveHistory.LOCATIONID) as LOC_ID\nFROM  {Config.sourceDatabase}.MoveHistory as MoveHistory\nWHERE MoveHistory._deleted_ = 'F'  \n \n\"\"\")",
