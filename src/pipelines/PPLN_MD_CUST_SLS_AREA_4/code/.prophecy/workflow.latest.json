          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n'{Config.sourceSystem}'   as SRC_SYS_CD\n,knvv.kunnr  as CUST_NUM\n,knvv.vtweg as DSTR_CHNL_CD\n,knvv.spart as DIV_CD\n,knvv.vkorg as SLORG_NUM\n,trim(knvv.waers) AS CRNCY_CD\n,trim(knvv.kdgrp) as CUST_GRP_CD\n,trim(knvv.KVGR1) AS CUST_GRP_1\n,trim(knvv.KVGR2) AS CUST_GRP_2\n,trim(knvv.KVGR3) AS CUST_GRP_3\n,trim(knvv.KVGR4) AS CUST_GRP_4\n,trim(knvv.KVGR5) AS CUST_GRP_5\n,trim(knvv.LIFSD) AS CUST_DELV_BLK\n,trim(knvv.KZTLF) AS PRTL_DELV_ITM_LVL\n,trim(knvv.CHSPL) AS BTCH_SPLT_ALLW\n,trim(knvv.BEGRU) AS AUTH_GRP\n,trim(knvv.VERSG) AS CUST_STATS_GRP\n,trim(knvv.KONDA) AS CUST_PRC_GRP\n,trim(knvv.ZTERM) AS PMT_TERM\nFROM {Config.sourceDatabase}.KNVV KNVV\nWHERE\n   KNVV._deleted_ = 'F' and mandt=(SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS\n WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
