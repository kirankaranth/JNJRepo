          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    vttk.tknum AS SHIP_NUM,\n    CASE\n        WHEN vttk.dtabf = '00000000' THEN NULL\n        ELSE to_timestamp(vttk.dtabf, 'yyyyMMdd')\n    END AS ACTL_SHIP_DTTM,\n    CASE\n        WHEN vttk.dpabf = '00000000' THEN NULL\n        ELSE to_timestamp(vttk.dpabf, 'yyyyMMdd')\n    END AS PLAN_SHIP_DTTM,\n    vttk.vbtyp AS SLS_ORDR_CAR_CD,\n    vttk.signi AS SHIP_CNTNR_NUM,\n    vttk.sttrg AS SHIP_STS_CD,\n    vttk.shtyp AS SHIP_TYPE_CD\nFROM {Config.sourceDatabase}.vttk vttk\nWHERE vttk._deleted_='F';  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
