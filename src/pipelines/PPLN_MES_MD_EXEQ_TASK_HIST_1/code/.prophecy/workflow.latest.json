          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(EXECUTETASKHISTORY.EXECUTETASKHISTORYID) as EXEQ_TASK_HIST_ID\n, TRIM(EXECUTETASKHISTORY.HISTORYMAINLINEID) as HIST_MNLINE_ID\n, TRIM(EXECUTETASKHISTORY.CARRIERID) as CARR_ID\n, INT(EXECUTETASKHISTORY.CHANGECOUNT) as CHG_CNT\n, TRIM(EXECUTETASKHISTORY.ETHCOMMENTS) as COMT_TXT\n, CAST(EXECUTETASKHISTORY.COMPLETETASKFORBATCH as BOOLEAN ) as CMPLT_TASK_FOR_BTCH_IND\n, CAST(EXECUTETASKHISTORY.ETH_CORRECTIONAPPLIED as BOOLEAN ) as CORR_APLD_IND\n, CAST(NULL AS STRING) as DVC_ID\n, TRIM(EXECUTETASKHISTORY.DOCUMENTSETID) as DOC_SET_ID\n, TRIM(EXECUTETASKHISTORY.ELECTRONICPROCEDUREID) as ELCTRNC_PCDR_ID\n, CAST(NULL AS STRING) as EMPS_PERFM_WRK_TXT\n, TRIM(EXECUTETASKHISTORY.EXPORTIMPORTKEY) as EXPT_IMPT_KEY_VAL\n, TRIM(EXECUTETASKHISTORY.HISTORYID) as HIST_ID\n, TRIM(EXECUTETASKHISTORY.INSTRUCTION) as INSTR_TXT\n, TRIM(EXECUTETASKHISTORY.ISSUEDIFFERENCEREASONID) as ISS_DIFF_RSN_ID\n, TRIM(EXECUTETASKHISTORY.MATERIALCONTAINERID) as MATL_CNTNR_ID\n, TRIM(EXECUTETASKHISTORY.CDOTYPEID) as OBJ_TYPE_ID\n, CAST(EXECUTETASKHISTORY.PASS as BOOLEAN ) as PAS_IND\n, CAST(NULL AS STRING) as PC_ID\n, INT(EXECUTETASKHISTORY.SEQUENCE) as SEQ_NBR\n, TRIM(EXECUTETASKHISTORY.SPECID) as SPEC_ID\n, TRIM(EXECUTETASKHISTORY.TASKCOMMENTS) as TASK_COMT_TXT\n, TRIM(EXECUTETASKHISTORY.TASKID) as TASK_ID\n, TRIM(EXECUTETASKHISTORY.TASKLISTID) as TASK_LIST_ID\n, INT(EXECUTETASKHISTORY.TASKLISTSEQUENCE) as TASK_LIST_SEQ_NBR\n, TRIM(EXECUTETASKHISTORY.TASKTYPE) as TASK_TYPE_CD\n, TRIM(EXECUTETASKHISTORY.TRAININGREQGROUPID) as TRAIN_REQ_GRP_ID\n, TRIM(EXECUTETASKHISTORY.TXNID) as TXN_ID\n, TRIM(EXECUTETASKHISTORY.WORKCELLID) as WRKCELL_ID\n, TRIM(EXECUTETASKHISTORY.WORKSTATIONID) as WSTN_ID\nFROM {Config.sourceDatabase}.EXECUTETASKHISTORY as EXECUTETASKHISTORY\nWhere EXECUTETASKHISTORY._deleted_ = 'F'  \n \n\"\"\")",
