          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(ESIGHISTORYSUMMARY.ESIGHISTORYSUMMARYID) as ESIGN_HIST_SUM_ID\n, TRIM(ESIGHISTORYSUMMARY.HISTORYMAINLINEID) as HIST_MNLINE_ID\n, INT(ESIGHISTORYSUMMARY.CHANGECOUNT) as CHG_CNT\n, TRIM(ESIGHISTORYSUMMARY.COSIGNERROLEID) as COSIGN_ROLE_ID\n, TRIM(ESIGHISTORYSUMMARY.ESIGREQDETAILID) as ESIGN_REQ_DTL_ID\n, CAST(NULL AS STRING) as EXPT_IMPT_KEY_VAL\n, TRIM(ESIGHISTORYSUMMARY.HISTORYID) as HIST_ID\n, TRIM(ESIGHISTORYSUMMARY.MEANINGID) as MEANING_ID\n, TRIM(ESIGHISTORYSUMMARY.CDOTYPEID) as OBJ_TYPE_ID\n, TRIM(ESIGHISTORYSUMMARY.ROLEID) as ROLE_ID\n, INT(ESIGHISTORYSUMMARY.SIGNATURECOUNT) as SGNTR_CNT\n, TRIM(ESIGHISTORYSUMMARY.TXNID) as TXN_ID\n, TRIM(ESIGHISTORYSUMMARY.VERIFICATIONMETHOD) as VERIF_METH_CD\nFROM  {Config.sourceDatabase}.ESIGHISTORYSUMMARY as ESIGHISTORYSUMMARY WHERE ESIGHISTORYSUMMARY._deleted_ =  'F'\n\n  \n \n\"\"\")",
