            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(EMPLOYEE.EMPLOYEEID) as EMP_ID\n, CAST(NULL AS BOOLEAN) as ALLW_MLT_SIGN_IN_IND\n, CAST(NULL AS BOOLEAN) as ALLW_MLT_TEAMS_IND\n, CAST(EMPLOYEE.ALLOWOVERRIDEOFSESSIONVALUES as BOOLEAN ) as ALLW_OVRD_OF_SESS_VAL_IND\n, CAST(EMPLOYEE.CANLOGIN as BOOLEAN ) as CN_LGN_IND\n, CAST(NULL AS BOOLEAN) as CN_UPDT_STERILIZATION_DT_IND\n, INT(EMPLOYEE.CHANGECOUNT) as CHG_CNT\n, TRIM(EMPLOYEE.CHANGESTATUSID) as CHG_STS_ID\n, CAST(NULL AS STRING) as DOC_MGR_PSWRD_TXT\n, CAST(NULL AS STRING) as DOC_MGR_USER_NM\n, TRIM(EMPLOYEE.DOMAINNAME) as DOM_NM\n, CAST(NULL AS BOOLEAN) as EDIT_APPR_SHT_IND\n, CAST(NULL AS BOOLEAN) as EDIT_LBR_SUP_IND\n, TRIM(EMPLOYEE.EMAILADDRESS) as EML_ADDR_TXT\n, TRIM(EMPLOYEE.DESCRIPTION) as EMP_DESC\n, CAST(NULL AS STRING) as EMP_LOG_IN_INFO_ID\n, TRIM(EMPLOYEE.EMPLOYEENAME) as EMP_NM\n, TRIM(EMPLOYEE.FULLNAME) as FULL_NM\n, TRIM(EMPLOYEE.HISTORYVIEWID) as HIST_VIEW_ID\n, TRIM(EMPLOYEE.ICONID) as ICON_ID\n, CAST(NULL AS STRING) as INTNL_ID\n, CAST(EMPLOYEE.ISFROZEN as BOOLEAN ) as IS_FRZN_IND\n, CAST(NULL AS STRING) as JDE_ADDR_BOOK_REC_VAL\n, TRIM(EMPLOYEE.LANGUAGEDICTIONARYID) as LANG_DICT_ID\n, TRIM(EMPLOYEE.MENUDEFINITIONID) as MENU_DEF_ID\n, CAST(EMPLOYEE.MODELERACCESS as BOOLEAN ) as MDLER_ACCS_IND\n, CAST(NULL AS STRING) as NOMENCULATURE_ID\n, TRIM(EMPLOYEE.NOTES) as NOTES_TXT\n, TRIM(EMPLOYEE.CDOTYPEID) as OBJ_TYPE_ID\n, TRIM(EMPLOYEE.PORTALMENUDEFINITIONID) as PORTL_MENU_DEF_ID\n, CAST(NULL AS STRING) as PORTL_MOBL_MENU_DEF_ID\n, TRIM(EMPLOYEE.PRIMARYORGANIZATIONID) as PRMRY_ORG_ID\n, TRIM(EMPLOYEE.SESSIONVALUESID) as SESS_VAL_ID\n, TRIM(EMPLOYEE.TERMINOLOGYDICTIONARYID) as TERMINOLOGY_DICT_ID\n, TRIM(EMPLOYEE.TRAININGPLANID) as TRAIN_PLAN_ID\n, TRIM(EMPLOYEE.UIPORTALPROFILEID) as UI_PORTL_PRFL_ID\n, TRIM(EMPLOYEE.USERCOMMENT) as USER_COMT_TXT\n, TRIM(EMPLOYEE.USERPROFILEID) as USER_PRFL_ID\n, TRIM(EMPLOYEE.WEBDRILLDOWNMENUDEFINITIONID) as WE