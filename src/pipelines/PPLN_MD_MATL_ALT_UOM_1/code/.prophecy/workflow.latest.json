            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT DISTINCT\n  '{Config.sourceSystem}'   AS SRC_SYS_CD\n, trim(marm.matnr) as MATL_NUM\n, trim(marm.meinh) as ALT_UOM_CD\n, null as BASE_UOM_CD\n, null as HARMO_UOM_CD\n, null as STD_UOM_CD\n, null as ENTRP_UOM_CD\n, cast(marm.umrez as DECIMAL(18,4)) as LCL_FACT_NUMRTR_MEAS\n, cast(marm.umren as DECIMAL(18,4)) as LCL_FACT_DENOM_MEAS\n, null as BASE_UOM_IND\n, null as BASE_UOM_MULT_QTY\n, cast(marm.umrez as DECIMAL(18,4)) as FACT_NUMRTR_MEAS\n, cast(marm.umren as DECIMAL(18,4)) as FACT_DENOM_MEAS\n, trim(marm.ean11) as GTIN_NUM\n, trim(marm.numtp) as GTIN_CAT_CD\n, cast(marm.breit as DECIMAL(18,4)) as WDTH_MEAS\n, trim(marm.meabm) as DIM_UOM_CD\n, cast(marm.brgew as DECIMAL(18,4)) as GRS_WT_MEAS\n, trim(marm.gewei) as WT_UOM_CD\n, cast(marm.laeng as DECIMAL(18,4)) as LGTH_MEAS\n, cast(marm.hoehe as DECIMAL(18,4)) as HGHT_MEAS\n, cast(marm.volum as DECIMAL(18,4)) as VOL_MEAS\n, trim(marm.voleh) as VOL_UOM_CD\n, null as NET_WT_MEAS\nfrom {Config.sourceDatabase}.marm  marm\nWHERE marm._deleted_='F'  \n \n\"\"\")",
