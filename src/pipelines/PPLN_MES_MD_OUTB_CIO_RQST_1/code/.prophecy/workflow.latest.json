          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n   '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(OUTBOUND_CIO_REQUESTS.HISTORYMAINLINEID) as HIST_MNLINE_ID\n, TRIM(OUTBOUND_CIO_REQUESTS.MESSAGE_TYPE) as MSG_TYPE_TXT\n, TRIM(OUTBOUND_CIO_REQUESTS.OPERATIONID) as OPR_ID\n, TRIM(OUTBOUND_CIO_REQUESTS.WORKFLOWSTEPID) as WRKF_STEP_ID\n, CAST(OUTBOUND_CIO_REQUESTS.CAN_RESUBMIT AS BOOLEAN) as CN_RESUBMT_IND\n, TRIM(OUTBOUND_CIO_REQUESTS.ERROR_MESSAGE) as ERR_MSG_TXT\n, case when outbound_cio_requests.last_activity_date = '0000000000' then CAST(NULL AS TIMESTAMP) else to_timestamp(left(outbound_cio_requests.last_activity_date ,14),\\\"yyyyMMddHHmmss\\\") end as LAST_ACTV_DTTM\n, TRIM(OUTBOUND_CIO_REQUESTS.CONTAINERID) as CNTNR_ID\n, TRIM(OUTBOUND_CIO_REQUESTS.STATUS) as STS_CD\nFROM  {Config.sourceDatabase}.OUTBOUND_CIO_REQUESTS as OUTBOUND_CIO_REQUESTS  WHERE OUTBOUND_CIO_REQUESTS._deleted_ = 'F'\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
