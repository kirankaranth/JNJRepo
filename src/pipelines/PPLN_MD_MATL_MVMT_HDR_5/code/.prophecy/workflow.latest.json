          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'   AS SRC_SYS_CD,\nTRIM(mkpf.mblnr) AS MATL_MVMT_NUM,\nTRIM(mkpf.mjahr) AS MATL_MVMT_YR,\nTRIM(mkpf.vgart) AS EV_TYPE_CD,\ncase when mkpf.bldat = '00000000' then null else to_timestamp(mkpf.bldat, \\\"yyyyMMdd\\\") end as MATL_MVMT_DTTM ,\ncase when mkpf.budat = '00000000' then null else to_timestamp(mkpf.budat, \\\"yyyyMMdd\\\") end as PSTNG_DTTM ,\nTRIM(mkpf.bktxt) AS MATL_MVMT_HDR_TXT,\nTRIM(blart) AS DOC_TYPE,\nNULL AS DOC_TYPE_DESC,\nTRIM(frbnr) AS BILL_OF_LANDG,\nCASE WHEN aedat = '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE To_timestamp(aedat,\\\"yyyyMMdd\\\") END AS LAST_CHG_DTTM\nFROM {Config.sourceDatabase}.mkpf mkpf\nWHERE mkpf._deleted_ = 'F' and  mkpf.mandt = '100'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
