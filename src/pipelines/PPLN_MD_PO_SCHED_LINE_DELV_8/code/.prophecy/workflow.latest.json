          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    select\nSRC_SYS_CD,\nPO_NUM,\nPO_LINE_NBR,\nDELV_SCHED_CNT_NBR,\nDELV_DTTM,\nSCHD_QTY,\nRECV_QTY,\nSTK_TFR_RECV_QTY,\nMRP_ADJ_QTY,\nSTAT_DELV_DTTM,\nCMT_QTY,\nORDER_TYPE,\nORDER_CO,\nORDER_SUF,\nPREV_QTY,\nCMT_DTTM,\nCAT_OF_DELV_DT,\nBTCH_NUM\nfrom\n(SELECT\n   (\n    SELECT\n      TRIM(SRC_SYS_CD)\n    FROM\n      l1.src_sys\n    WHERE\n      SRC_SYS_CD = '{src_sys_cd}'\n      AND src_sys.Active = TRUE\n  ) AS SRC_SYS_CD,\n  row_number() OVER (PARTITION BY F4311.PDDOCO,\n                                          F4311.PDLNID,\n                                          F4311.PDDCTO,\n            F4311.PDKCOO,\n            F4311.PDSFXO\n                             ORDER BY F4311.PDDOCO) AS row_num,\n  F4311.PDDOCO AS PO_NUM,\n  F4311.PDLNID AS PO_LINE_NBR,\n  \\\"#\\\" AS DELV_SCHED_CNT_NBR,\n  TO_TIMESTAMP(CASE\n    WHEN LOWER(TRIM(F4311.PDPDDJ)) = 'null'\n    OR TRIM(F4311.PDPDDJ) = ''\n    OR TRIM(F4311.PDPDDJ) = '0' THEN NULL\n    ELSE to_timestamp(\n      substr(\n        CAST(\n          date_add(\n            concat(\n              substr(\n                CAST(\n                  CAST(TRIM(F4311.PDPDDJ) AS INT) + 1900000 AS STRING\n                ),\n                1,\n                4\n              ),\n              '-01-01'\n            ),\n            CAST(\n              substr(\n                CAST(\n                  CAST(TRIM(F4311.PDPDDJ) AS INT) + 1900000 AS string\n                ),\n                5\n              ) AS INT\n            ) -1\n          ) AS STRING\n        ),\n        1,\n        10\n      ),\n      'yyyy-MM-dd'\n    )\n  END\n  )  AS DELV_DTTM,\n  CAST(trim(F4311.PDUORG) AS DECIMAL(18,4)) AS SCHD_QTY,\n  CAST(NULL AS DECIMAL(18,4)) AS RECV_QTY,\n  CAST(trim(F4311.PDUREC) AS DECIMAL(18,4)) AS STK_TFR_RECV_QTY,\n  CAST(NULL AS DECIMAL(18,4)) AS MRP_ADJ_QTY,\n  TO_TIMESTAMP(CASE\n    WHEN LOWER(TRIM(F4311.PDOPDJ)) = 'null'\n    OR TRIM(F4311.PDOPDJ) = ''\n    OR TRIM(F4311.PDOPDJ) = '0' THEN NULL\n    ELSE to_timestamp(\n      substr(\n        CAST(\n          date_add(\n            concat(\n              substr(\n                CAST(\n                  CAST(TRIM(F4311.PDOPDJ) AS INT) + 1900000 AS STRING\n                ),\n                1,\n                4\n              ),\n              '-01-01'\n            ),\n            CAST(\n              substr(\n                CAST(\n                  CAST(TRIM(F4311.PDOPDJ) AS INT) + 1900000 AS string\n                ),\n                5\n              ) AS INT\n            ) -1\n          ) AS STRING\n        ),\n        1,\n        10\n      ),\n      'yyyy-MM-dd'\n    )\n  END\n  )  AS STAT_DELV_DTTM,\n  CAST(NULL AS DECIMAL(18,4)) AS CMT_QTY,\n  F4311.PDDCTO AS ORDER_TYPE,\n  F4311.PDKCOO AS ORDER_CO,\n  F4311.PDSFXO AS ORDER_SUF,\n  CAST(NULL AS DECIMAL(18,4)) as PREV_QTY,\n  CAST(NULL AS TIMESTAMP)  as CMT_DTTM,\n  NULL as CAT_OF_DELV_DT,\n  NULL AS BTCH_NUM\nFROM\n  {Config.sourceDatabase}.f4311\nWHERE\n  f4311._deleted_ = 'F')\n  where row_num=1\n\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
