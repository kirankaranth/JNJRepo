          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\nTRIM(AFFL.aplzl) AS ORDR_RTNG_CTR_NUM,\nTRIM(AFFL.aufpl) AS ORDR_RTNG_NUM,\nTRIM(AFFL.aennr) AS CHG_NUM,\nTRIM(AFFL.bknt1) AS STRT_NDE_NUM,\nTRIM(AFFL.bknt2) AS END_NDE_NUM,\nTRIM(AFFL.bschl1) AS BRNCH_OPER_NUM,\nTRIM(AFFL.bschl2) AS RETRN_OPER_NUM,\nTRIM(AFFL.flgat) AS RTNG_SQNC_CTGRY_CD,\nCAST(TRIM(AFFL.losbs) AS DECIMAL(18,4)) AS TO_LT_SZ_QTY,\nCAST(TRIM(AFFL.losvn) AS DECIMAL(18,4)) AS FROM_LT_SZ_QTY,\nTRIM(AFFL.plnal) AS RNTG_GRP_CNTR_NBR,\nTRIM(AFFL.plnfl) AS RTNG_SQNC_NUM,\nTRIM(AFFL.plnnr) AS RNTG_GRP_CD,\nTRIM(AFFL.plnty) AS RTNG_TYP_CD,\nTRIM(AFFL.zaehl) AS RTNG_SQNC_VRSN_CNTR_NBR\nFROM {Config.sourceDatabase}.AFFL AFFL \nWHERE AFFL._deleted_ = 'F'  \n \n\"\"\")",
