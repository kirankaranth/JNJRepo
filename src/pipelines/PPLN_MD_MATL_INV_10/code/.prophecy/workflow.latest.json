          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    'F41021' AS SRC_TBL_NM,\n    F4101.imlitm AS MATL_NUM,\n    f41021.limcu AS PLNT_CD,\n    f41021.lilocn AS SLOC_CD,\n    f41021.lilotn AS BTCH_NUM,\n    f41021.lilots AS BTCH_STS_CD,\n    CASE WHEN length(Trim(f41021.lipbin)) = 0 OR f41021.lipbin IS NULL THEN '#' ELSE f41021.lipbin END AS SPCL_STCK_IND,\n    '#' AS PRTY_NUM,\n    CAST(\n            CASE\n                WHEN TRIM(COALESCE(LILOTS, '')) = '' THEN TRIM(LIPQOH)/1000\n                ELSE 0\n                END  AS DECIMAL(18, 4))\n        AS UNRSTRCTD_STCK,\n CAST(TRIM(f41021.liqttr)/1000 AS DECIMAL(18, 4)) AS IN_TRNSFR_STCK,\n    CAST(TRIM(f41021.liqtin)/1000 AS DECIMAL(18, 4)) AS QLTY_INSP_STCK,\n    CAST(\n            CASE\n                WHEN TRIM(COALESCE(LILOTS, '')) <> '' THEN TRIM(LIPQOH)/1000\n                ELSE 0\n                END AS DECIMAL(18, 4))\n         AS RSTRCTD_STCK,\n    NULL AS   BLCKD_STCK,\n    NULL AS   CRT_DTTM,\n    NULL AS   RTRNS,\n    F4101.imuom1 AS BASE_UOM_CD,\n    NULL AS STO_IN_TRNST_QTY,\n    NULL AS PLNT_IN_TRNST_QTY,\n NULL AS FISC_YR_OF_CUR_PER,\n    NULL AS CUR_PER\nFROM\n    {Config.sourceDatabase}.f41021\n  INNER JOIN {Config.sourceDatabase}.f4101 ON f41021.liitm = f4101.imitm\n    AND f4101._deleted_ = 'F'\nWHERE\n    f41021._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
