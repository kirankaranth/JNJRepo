          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\nLFM1.lifnr AS SUP_NUM,\nLFM1.ekorg AS PRCHSNG_ORG_NUM,\nCASE WHEN LFM1.erdat= '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE TO_TIMESTAMP(LFM1.erdat,'yyyyMMdd') END AS CRT_ON_DTTM,\nTRIM(LFM1.sperm) AS PRCH_BLK_IND,\nTRIM(LFM1.loevm) AS DEL_IND,\nTRIM(LFM1.waers) AS CRNCY_CD,\nTRIM(LFM1.zterm) AS PMT_TERM_CD,\nTRIM(LFM1.inco1) AS INCOTERM1_CD,\nTRIM(LFM1.inco2) AS INCOTERM2_CD,\nTRIM(LFM1.kalsk) AS PRC_PCDR_CD,\nTRIM(LFM1.meprf) AS PRC_CNTL_CD,\nTRIM(LFM1.xersy) AS EVAL_RCPT_SETLM_CD,\nTRIM(LFM1.kzret) AS RTRN_VEND_IND,\nTRIM(LFM1.bstae) AS CNFRM_CD,\nTRIM(LFM1.ernam) AS NM_OF_PRSN_RESP_CREAT_OBJ,\nTRIM(LFM1.webre) AS GR_BAS_INVC_VERIF,\nTRIM(LFM1.kzaut) AS AUTO_GNR_OF_PO_ALLW,\nTRIM(LFM1.xersr) AS AUTO_EVAL_RCPT_SETLM,\nNULL AS OWN_EXPLN_OF_TERM_OF_PMT,\nTRIM(LFM1.vsbed) AS SHIPPING_COND_CD\nFROM {Config.sourceDatabase}.LFM1 LFM1\nWHERE LFM1._deleted_ = 'F' AND  LFM1.MANDT = 232  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
