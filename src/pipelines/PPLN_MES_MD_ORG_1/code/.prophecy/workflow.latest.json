          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(ORGANIZATION.ORGANIZATIONID) as ORG_ID\n, TRIM(ORGANIZATION.PARENTORGANIZATIONID) as PARNT_ORG_ID\n, INT(ORGANIZATION.CHANGECOUNT) as CHG_CNT\n, TRIM(ORGANIZATION.CHANGEHISTORYID) as CHG_HIST_ID\n, CAST(ORGANIZATION.COLLECTESIGFORALLQUALITYTXNS as BOOLEAN ) as COLL_ESIG_FOR_ALL_QUAL_TXNS_IN\n, TRIM(ORGANIZATION.ICONID) as ICON_ID\n, CAST(ORGANIZATION.ISFROZEN as BOOLEAN ) as IS_FRZN_IND\n, TRIM(ORGANIZATION.NOTES) as NOTES_TXT\n, INT(ORGANIZATION.CDOTYPEID) as OBJ_TYPE_ID\n, TRIM(ORGANIZATION.ORGANIZATIONNAME) as ORG_NM\n, TRIM(ORGANIZATION.DESCRIPTION) as ORG_DESC\n, TRIM(ORGANIZATION.ORGANIZATIONNUMBER) as ORG_NUM\n, TRIM(ORGANIZATION.PORTALHOMEPAGEID) as PORTL_HOME_PG_ID\n, TRIM(ORGANIZATION.PRINTQUEUEID) as PRT_QUE_ID\n, TRIM(ORGANIZATION.SMTPTRANSPORTID) as SMTP_TRNSP_ID\n, CAST(ORGANIZATION.USERREQUIREDFORESIG as BOOLEAN ) as USER_REQ_FOR_ESIG_IND\nFROM  {Config.sourceDatabase}.ORGANIZATION as ORGANIZATION WHERE ORGANIZATION._deleted_ =  'F'\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
