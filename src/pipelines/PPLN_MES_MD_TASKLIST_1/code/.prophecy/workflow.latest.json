          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT(\n  '{Config.sourceSystem}' AS SRC_SYS_CD,\nTRIM(Tasklist.TASKLISTID) as TASKLIST_ID,\nTRIM(Tasklist.TASKLISTBASEID) as TASKLIST_BASE_ID,\ncast (TRIM(Tasklist.BPINTERMEDIATEQTY) as DECIMAL(18,4)) as BTCH_INTRM_QTY,\nTRIM(Tasklist.BPINTERMEDIATEUOMID) as BTCH_INTRM_UOM_CD,\ncast(TRIM(Tasklist.CHANGECOUNT) as INT) as CHG_CNT,\nTRIM(Tasklist.CHANGEHISTORYID) as CHG_HIST_ID,\nTRIM(Tasklist.ECO) as ENG_CHG_ORDR_NUM,\nTRIM(Tasklist.EXECUTIONMODE) as EXEC_MODE_CD,\nTRIM(Tasklist.ICONID) as ICON_ID,\nTRIM(Tasklist.INSTRUCTION) as INSTR_TXT,\nCAST(TRIM(Tasklist.ISFROZEN) AS BOOLEAN) as IS_FRZN_IND,\nTRIM(Tasklist.NOTES) as NOTES_TXT,\nTRIM(Tasklist.CDOTYPEID) as OBJ_TYPE_ID,\nTRIM(Tasklist.PREREQUISITETASKLISTID) as PRQST_TASK_LIST_ID,\nTRIM(Tasklist.REPORTINSTRUCTION) as RPT_INSTR_TXT,\nTRIM(Tasklist.STATUS) as STS_CD,\nTRIM(Tasklist.DESCRIPTION) as TASKLIST_DESC,\nTRIM(Tasklist.TASKLISTREVISION) as TASKLIST_RVSN_ID,\nTRIM(Tasklist.WIPMSGDEFMGRID) as WIP_MSG_DEF_MGR_ID,\nTRIM(Tasklist.WORKSTATIONGROUPID) as WRK_STN_GRP_ID,\nTRIM(Tasklist.WORKSTATIONID) as WRK_STN_ID\nFROM {Config.sourceDatabase}.Tasklist Tasklist\nWhere Tasklist._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
