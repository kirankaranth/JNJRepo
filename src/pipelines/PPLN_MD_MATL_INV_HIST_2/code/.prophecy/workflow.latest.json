          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    \nwith mysub\nas\n(select 'MCHBH' as src_tbl_nm\n, MCHBH.matnr as matl_num, MCHBH.werks as plnt_cd, MCHBH.lgort as sloc_cd, MCHBH.charg as btch_num, null spcl_stck_ind, null as prty_num, MCHBH.lfgja as PSTNG_YR, MCHBH.lfmon as PSTNG_PER, MCHBH.clabs as unrstrctd_stck, MCHBH.cumlm in_trnsfr_stck, MCHBH.cinsm as qlty_insp_stck, MCHBH.ceinm as rstrctd_stck, MCHBH.cspem as blckd_stck, MCHBH.cretm as rtrns\nfrom {Config.sourceDatabase}.MCHBH MCHBH\nwhere MCHBH._deleted_ = 'F' and MCHBH.MANDT = '020'\nUNION ALL\nselect 'MARDH' as src_tbl_nm\n, MARDH.matnr as matl_num, MARDH.werks as plnt_cd, MARDH.lgort as sloc_cd, null as btch_num, null  spcl_stck_ind, null as prty_num, MARDH.lfgja as PSTNG_YR, MARDH.lfmon as PSTNG_PER, MARDH.labst as unrstrctd_stck, MARDH.umlme in_trnsfr_stck, MARDH.insme as qlty_insp_stck, MARDH.einme as rstrctd_stck, MARDH.speme as blckd_stck, MARDH.retme as rtrns\nfrom {Config.sourceDatabase}.MARDH MARDH\nwhere MARDH._deleted_ = 'F' and MARDH.MANDT = '020'\nUNION ALL\nselect 'MSKUH' as src_tbl_nm\n, MSKUH.matnr as matl_num, MSKUH.werks as plnt_cd, null as sloc_cd, MSKUH.charg as btch_num, MSKUH.sobkz spcl_stck_ind, MSKUH.kunnr as prty_num, MSKUH.lfgja as PSTNG_YR, MSKUH.lfmon as PSTNG_PER, MSKUH.kulab as unrstrctd_stck, MSKUH.kuuml in_trnsfr_stck, MSKUH.kuins as qlty_insp_stck, MSKUH.kuein as rstrctd_stck, null as blckd_stck, null as rtrns\nfrom {Config.sourceDatabase}.MSKUH MSKUH\nwhere MSKUH._deleted_ = 'F' and MSKUH.MANDT = '020'\nUNION ALL\nselect 'MSLBH' as src_tbl_nm\n, MSLBH.matnr as matl_num, MSLBH.werks as plnt_cd, null as sloc_cd, MSLBH.charg as btch_num, MSLBH.sobkz spcl_stck_ind, MSLBH.lifnr as prty_num, MSLBH.lfgja as PSTNG_YR, MSLBH.lfmon as PSTNG_PER, MSLBH.lblab as unrstrctd_stck, MSLBH.lbuml  in_trnsfr_stck, MSLBH.lbins as qlty_insp_stck, MSLBH.lbein as rstrctd_stck, null as blckd_stck, null as rtrns\nfrom {Config.sourceDatabase}.MSLBH MSLBH\nwhere MSLBH._deleted_ = 'F' and MSLBH.MANDT = '020'\nUNION ALL\nselect 'MKOLH' as src_tbl_nm\n, MKOLH.matnr as matl_num, MKOLH.werks as plnt_cd, lgort as sloc_cd, MKOLH.charg as btch_num, MKOLH.sobkz spcl_stck_ind, MKOLH.lifnr as prty_num, MKOLH.lfgja as PSTNG_YR, MKOLH.lfmon as PSTNG_PER, MKOLH.slabs as unrstrctd_stck, null as in_trnsfr_stck, MKOLH.sinsm as qlty_insp_stck, MKOLH.seinm as rstrctd_stck, MKOLH.sspem as blckd_stck, null as rtrns\nfrom {Config.sourceDatabase}.MKOLH MKOLH\nwhere MKOLH._deleted_ = 'F' and MKOLH.MANDT = '020'\n)\nselect\n  '{Config.sourceSystem}'  AS SRC_SYS_CD\n, mysub.src_tbl_nm   as SRC_TBL_NM\n, mysub.MATL_NUM     AS MATL_NUM\n, mysub.PLNT_CD      AS PLNT_CD\n, coalesce(mysub.SLOC_CD,'#')    AS SLOC_CD\n, coalesce(mysub.BTCH_NUM,'#')    AS BTCH_NUM\n, coalesce(mysub.SPCL_STCK_IND,'#')   AS SPCL_STCK_IND\n, coalesce(mysub.PRTY_NUM,'#')   AS PRTY_NUM\n, cast(mysub.PSTNG_YR as int) AS PSTNG_YR\n, cast(mysub.PSTNG_PER as int) AS PSTNG_PER\n, cast(mysub.UNRSTRCTD_STCK AS DECIMAL(18,4))  AS UNRSTRCTD_STCK\n, cast(mysub.IN_TRNSFR_STCK AS DECIMAL(18,4)) AS IN_TRNSFR_STCK\n, cast(mysub.QLTY_INSP_STCK AS DECIMAL(18,4)) AS QLTY_INSP_STCK\n, cast(mysub.RSTRCTD_STCK AS DECIMAL(18,4))    AS RSTRCTD_STCK\n, cast(mysub.BLCKD_STCK AS DECIMAL(18,4))  AS BLCKD_STCK\n, cast(mysub.RTRNS AS DECIMAL(18,4))   AS RTRNS\n, MARA.meins  AS BASE_UOM_CD\n, null   AS STO_IN_TRNST_QTY\n, null   AS PLNT_IN_TRNST_QTY\nfrom mysub mysub\nLEFT JOIN {Config.sourceDatabase}.MARA MARA on MARA.MATNR = mysub.MATL_NUM AND MARA._deleted_ = 'F' and MARA.MANDT = '020'\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
