          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    with mandt_filter AS\n(\n    (SELECT L1_PARAMETERS.PARAM_VAL\n    from l1_md_prophecy_config.l1_parameters L1_PARAMETERS\n    WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')\n)\nSELECT\n  '{Config.sourceSystem}'  AS SRC_SYS_CD,\nTRIM(VBPA.POSNR) AS SLS_DOC_ITEM_NBR,\nTRIM(VBPA.VBELN) AS SLS_DOC_ID,\nTRIM(VBPA.PARVW) AS PTNR_FUNC_CD,\nTRIM(VBAK.BUKRS_VF) AS COMPANY_CD,\nTRIM(VBAK.AUART) AS SLS_ORDR_TYPE_CD,\nTRIM(VBPA.PARVW_FF) AS FURTHER_PTNR_IND,\nTRIM(VBPA.LIFNR) AS SUP_NUM,\nTRIM(VBPA.ADRNR) AS ADDR_USG_CD,\nTRIM(VBPA.KUNNR) AS CUST_NUM,\nTRIM(VBPA.LAND1) AS CTRY_CD\nFROM {Config.sourceDatabase}.VBPA VBPA\nleft join {Config.sourceDatabase}.VBAK VBAK on VBPA.VBELN=VBAK.VBELN AND VBAK._deleted_ = 'F' AND  VBAK.MANDT = (SELECT param_val FROM mandt_filter)\nWHERE \nVBPA._deleted_ = 'F' AND  VBPA.MANDT =(SELECT param_val FROM mandt_filter)\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
