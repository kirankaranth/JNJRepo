          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    select\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\n'f4108'      AS SRC_TBL_NM,\nf4108.iolitm AS MATL_NUM,\nf4108.iolotn AS BTCH_NUM,\nf4108.iomcu  AS PLNT_CD,\nNULL as DEL_IND,\n\nto_timestamp(date_add(cast(substring(ioohdj + 1900000,1,4) AS Date),cast(substring(ioohdj,4,3) as int)-1))  AS CRT_DTTM,\n\n\nto_timestamp(date_add(cast(substring(ioupmj + 1900000,1,4) AS Date),cast(substring(ioupmj,4,3) as int)-1)) AS CHG_DTTM,\n\n\nto_timestamp(date_add(cast(substring(iodlej + 1900000,1,4) AS Date),cast(substring(iodlej,4,3) as int)-1)) AS AVAIL_DTTM,\n\n\nto_timestamp(date_add(cast(substring(iommej + 1900000,1,4) AS Date),cast(substring(iommej,4,3) as int)-1)) AS BTCH_EXP_DTTM,\n\ntrim(f4108.iolots) as BTCH_STS_CD,\nNULL as BTCH_LAST_STS_DTTM,\ntrim(f4108.iovend) as SUP_NUM,\ntrim(f4108.iorlot) as SUP_BTCH_NUM,\nNULL as BTCH_LAST_GR_DTTM,\nNUll as BTCH_MFG_DTTM,\ntrim(f4108.iodcto) as BTCH_TYPE,\ntrim(f4108.iochdf) as SUI_IND,\ntrim(f4108.iolotg) as LOT_GRADE,\nNULL as PARENT_CODE,\nf4108.ioitm as SHRT_MATL_NUM\nfrom {Config.sourceDatabase}.f4108\nWHERE f4108._deleted_='F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
