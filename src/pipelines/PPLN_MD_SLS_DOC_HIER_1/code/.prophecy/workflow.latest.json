          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}' AS  SRC_SYS_CD,\n VBAK.BUKRS_VF AS CO_CD,\nVBFA.VBELV AS PREV_DOC_NUM,\nVBFA.POSNV AS PREV_DOC_LINE_NBR,\nVBFA.VBELN AS SUBSQ_DOC_NUM,\nVBFA.POSNN AS SUBSQ_DOC_LINE_NBR,\nVBFA.VBTYP_N AS SUBSQ_DOC_CAT_CD,\nVBAK.AUART   AS PREV_DOC_TYPE_CD,\nVBFA.VBTYP_V AS PREV_DOC_CAT_CD,\nCAST(TRIM(VBFA.RFMNG) AS decimal(18,4)) AS REF_QTY,\nTRIM(VBFA.MEINS) AS BASE_UOM_CD,\nTRIM(VBFA.RFWRT) AS REF_AMT,\nTRIM(VBFA.WAERS) AS CRNCY_CD,\nCASE  WHEN VBFA.ERDAT = '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE to_timestamp(CONCAT(VBFA.ERDAT,VBFA.ERZET),'yyyyMMddHHmmss')\nEND AS CRT_DTTM,\nTRIM(VBFA.MATNR) AS MATL_NUM,\nCASE WHEN VBFA.aedat = '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE to_timestamp(VBFA.aedat, 'yyyyMMdd') END AS CHG_DTTM,\nTRIM(VBFA.BDART) AS REQ_TYPE_CD,\nTRIM(VBFA.PLART) AS PLNG_TYPE_CD,\nTRIM(VBFA.STUFE) AS LVL_CD,\nTRIM(VBFA.LGNUM) AS WHSE_CD,\nTRIM(VBFA.FKTYP) AS BILL_CAT_CD,\nCAST(TRIM(VBFA.BRGEW) AS decimal(18,4)) AS GRS_WT_MEAS,\nTRIM(VBFA.GEWEI)  AS WT_UOM_CD,\nCAST(TRIM(VBFA.VOLUM) AS decimal(18,4)) AS VOL_MEAS,\nTRIM(VBFA.VOLEH) AS VOL_UOM_CD,\nTRIM(VBFA.VRKME) AS SLS_UOM_CD,\nTRIM(VBFA.SOBKZ) AS SPL_STK_TYPE_CD,\nTRIM(VBFA.SONUM) AS SPL_STK_NUM,\nCAST(TRIM(VBFA.NTGEW) AS decimal(18,4)) AS NET_WT_MEAS,\nTRIM(VBFA.WBSTA) AS GM_STS_CD,\nTRIM(VBFA.CMETH) AS QTY_CONV_CD,\nTRIM(VBFA.MJAHR) AS MATL_MVMT_YR,\n'#' AS SD_UNIQ_DOC_RL_ID\n  FROM\n    {Config.sourceDatabase}.VBFA\n    LEFT JOIN {Config.sourceDatabase}.VBAK ON VBAK.vbeln = VBFA.vbeln AND VBAK._deleted_= 'F' AND VBAK.mandt = '100'\n LEFT JOIN {Config.sourceDatabase}.VBAP ON VBFA.VBELN = VBAP.VBELN AND VBFA.POSNV = VBAP.POSNR AND VBAP._deleted_= 'F' and VBAP.mandt = '100'\nWHERE\n  VBFA._deleted_ = 'F' and VBFA.mandt = '100'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
