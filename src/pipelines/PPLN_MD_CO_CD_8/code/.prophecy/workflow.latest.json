          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  KNB1.bukrs AS CO_CD,\n  KNB1.kunnr AS CUST_NUM,\n  '{Config.sourceSystem}'  AS SRC_SYS_CD,\n  TRIM(KNB1.ekvbd) AS BUY_GRP_CD,\n  TRIM(KNB1.fdgrv) AS PLNG_CUST_IND,\n  TRIM(KNB1.zterm) AS TERM_PMT_KEY\nFROM\n  {Config.sourceDatabase}.KNB1 KNB1\nWHERE\n  KNB1._deleted_ = 'F'\n  AND KNB1.MANDT = (\n    SELECT\n      L1_PARAMETERS.PARAM_VAL\n    FROM\n      CONFIG.L1_PARAMETERS L1_PARAMETERS\n    WHERE\n      L1_PARAMETERS.ENV_NM = '{env}'\n      AND L1_PARAMETERS.PARAM_NM = 'mandt_filter'\n   )  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
