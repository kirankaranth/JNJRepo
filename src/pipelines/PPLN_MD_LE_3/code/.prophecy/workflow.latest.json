          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\nT001.BUKRS AS LE_CD,\nNULL AS LE_TYPE_IND,\nTRIM(T001.BUTXT) AS LE_NM,\nTRIM(T001.KTOPL) AS CHRT_OF_ACCT_CD,\nTRIM(T001.ORT01) AS LE_CITY_CD,\nTRIM(T001.STCEG) AS VAT_REGS_NUM,\nTRIM(T001.WAABW) AS MAX_EXCH_RT_DEVT,\nTRIM(T001.ADRNR) AS ADDR_CD,\nTRIM(T001.TXJCD) AS JURIS_CD,\nTRIM(T001.PERIV) AS FISC_YR_VRNT_CD,\nTRIM(T001.WAERS) AS LE_CRNCY_CD,\nTRIM(T001.LAND1) AS LE_CTRY_CD,\nTRIM(T002.SPRAS) AS LANG_CD,\nTRIM(TKA01.KOKRS)AS CNTL_AREA_CD,\nNULL AS CO,\nNULL AS INP_TAX_CD,\nNULL AS OUTP_TAX_CD,\nNULL AS CTRY_CHRT_OF_ACCT,\nNULL AS HYPER_INFLT_IND\nfrom {Config.sourceDatabase}.T001\nleft join {Config.sourceDatabase}.t002  on  t001.spras=t002.spras   and t002.spras= 'E'    and T002._deleted_ = 'F'\nleft join {Config.sourceDatabase}.tka01 on  t001.bukrs=tka01.kokrs  and tka01.mandt= '020' and Tka01._deleted_ = 'F'\nWHERE T001._deleted_ = 'F'\nAND T001.mandt = '020'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
