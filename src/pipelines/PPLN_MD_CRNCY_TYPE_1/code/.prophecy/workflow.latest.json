          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    WITH filter_table AS\n    ( SELECT '100' AS MANDT, 'bba' AS SRC_SYS_CD\n      UNION ALL\n      SELECT '100' AS MANDT, 'bbn' AS SRC_SYS_CD\n      UNION ALL\n      SELECT '100' AS MANDT, 'bbl' AS SRC_SYS_CD\n      UNION ALL\n      SELECT '232' AS MANDT, 'geu' AS SRC_SYS_CD\n      UNION ALL\n      SELECT '100' AS MANDT, 'mrs' AS SRC_SYS_CD\n      UNION ALL\n      SELECT '020' AS MANDT, 'p01' AS SRC_SYS_CD\n      UNION ALL\n      SELECT '800' AS MANDT, 'tai' AS SRC_SYS_CD\n    )\nSELECT\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\nCKMLCUR.curtp AS CRNCY_TYPE_CD,\nCKMLCUR.sprsl AS LANG_CD,\nCKMLCUR.ddtext AS CRNCY_TYPE_SHRT_NM,\nCKMLCUR.text60 AS CRNCY_TYPE_LONG_NM,\nCKMLCUR.text10 AS CRNCY_TYPE_MED_NM\nFROM {Config.sourceDatabase}.CKMLCUR CKMLCUR \nWHERE CKMLCUR._deleted_ = 'F'\nAND CKMLCUR.mandt = ( SELECT MANDT\n                    FROM filter_table\n                    WHERE SRC_SYS_CD='{src_sys_cd}')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
