          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(CONTAINERDEFECTHISTORYDETAIL.ContainerDefectHistoryDetailId) as CNTNR_DEFCT_HIST_DTL_ID\n, TRIM(CONTAINERDEFECTHISTORYDETAIL.ContainerId) as CNTNR_ID\n, TRIM(CONTAINERDEFECTHISTORYDETAIL.DefectHistoryId) as DEFCT_HIST_ID\n, INT(CONTAINERDEFECTHISTORYDETAIL.ChangeCount) as CHG_CNT\n, TRIM(CONTAINERDEFECTHISTORYDETAIL.Comments) as COMT_TXT\n, INT(CONTAINERDEFECTHISTORYDETAIL.DefectCount) as DEFCT_CNT\n, TRIM(CONTAINERDEFECTHISTORYDETAIL.HistoryId) as HIST_ID\n, TRIM(CONTAINERDEFECTHISTORYDETAIL.CDOTypeId) as OBJ_TYPE_ID\n, TRIM(CONTAINERDEFECTHISTORYDETAIL.ReasonCodeId) as RSN_CD\n, TRIM(CONTAINERDEFECTHISTORYDETAIL.TxnId) as TXN_ID\nFROM {Config.sourceDatabase}.CONTAINERDEFECTHISTORYDETAIL as CONTAINERDEFECTHISTORYDETAIL\nWHERE CONTAINERDEFECTHISTORYDETAIL._deleted_ =  'F'\n  \n \n\"\"\")",
