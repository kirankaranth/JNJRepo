          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n   (\n    SELECT\n      TRIM(SRC_SYS_CD)\n    FROM\n      l1.src_sys\n    WHERE\n      SRC_SYS_CD = '{src_sys_cd}'\n      AND src_sys.Active = TRUE\n  ) AS SRC_SYS_CD,\n  f4311_adt.PDDOCO AS PO_NUM,\n  f4311_adt.PDLNID AS PO_LINE_NBR,\n  \\\"#\\\" AS DELV_SCHED_CNT_NBR,\n  TO_TIMESTAMP(CASE\n    WHEN LOWER(TRIM(f4311_adt.PDPDDJ)) = 'null'\n    OR TRIM(f4311_adt.PDPDDJ) = ''\n    OR TRIM(f4311_adt.PDPDDJ) = '0' THEN NULL\n    ELSE to_timestamp(\n      substr(\n        CAST(\n          date_add(\n            concat(\n              substr(\n                CAST(\n                  CAST(TRIM(f4311_adt.PDPDDJ) AS INT) + 1900000 AS STRING\n                ),\n                1,\n                4\n              ),\n              '-01-01'\n            ),\n            CAST(\n              substr(\n                CAST(\n                  CAST(TRIM(f4311_adt.PDPDDJ) AS INT) + 1900000 AS string\n                ),\n                5\n              ) AS INT\n            ) -1\n          ) AS STRING\n        ),\n        1,\n        10\n      ),\n      'yyyy-MM-dd'\n    )\n  END\n  )  AS DELV_DTTM,\n  CAST(trim(f4311_adt.PDUORG) AS DECIMAL(18,4)) AS SCHD_QTY,\n  CAST(NULL AS DECIMAL(18,4)) AS RECV_QTY,\n  CAST(trim(f4311_adt.PDUREC) AS DECIMAL(18,4)) AS STK_TFR_RECV_QTY,\n  CAST(NULL AS DECIMAL(18,4)) AS MRP_ADJ_QTY,\n  TO_TIMESTAMP(CASE\n    WHEN LOWER(TRIM(f4311_adt.PDOPDJ)) = 'null'\n    OR TRIM(f4311_adt.PDOPDJ) = ''\n    OR TRIM(f4311_adt.PDOPDJ) = '0' THEN NULL\n    ELSE to_timestamp(\n      substr(\n        CAST(\n          date_add(\n            concat(\n              substr(\n                CAST(\n                  CAST(TRIM(f4311_adt.PDOPDJ) AS INT) + 1900000 AS STRING\n                ),\n                1,\n                4\n              ),\n              '-01-01'\n            ),\n            CAST(\n              substr(\n                CAST(\n                  CAST(TRIM(f4311_adt.PDOPDJ) AS INT) + 1900000 AS string\n                ),\n                5\n              ) AS INT\n            ) -1\n          ) AS STRING\n        ),\n        1,\n        10\n      ),\n      'yyyy-MM-dd'\n    )\n  END\n  )  AS STAT_DELV_DTTM,\n  CAST(NULL AS DECIMAL(18,4)) AS CMT_QTY,\n  f4311_adt.PDDCTO AS ORDER_TYPE,\n  f4311_adt.PDKCOO AS ORDER_CO,\n  f4311_adt.PDSFXO AS ORDER_SUF,\n  CAST(NULL AS DECIMAL(18,4)) as PREV_QTY,\n  CAST(NULL AS TIMESTAMP)  as CMT_DTTM,\n  NULL as CAT_OF_DELV_DT,\n  NULL AS BTCH_NUM\nFROM\n  {Config.sourceDatabase}.f4311_adt\nWHERE\n  f4311_adt._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
