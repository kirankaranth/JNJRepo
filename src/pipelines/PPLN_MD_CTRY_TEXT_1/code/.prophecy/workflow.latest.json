          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    WITH ref_table AS\n  (SELECT '100' AS `MANDT`,\n          'bbn' AS `SRC_SYS_CD`\n   UNION ALL SELECT '100',\n                    'bbl'\n   UNION ALL SELECT '100',\n                    'mrs'\n   UNION ALL SELECT '100',\n                    'hcs'\n   UNION ALL SELECT '020',\n                    'p01'\n   UNION ALL SELECT '800',\n                    'tai'),\nfinal_table AS\n(\n  SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    TRIM(T005T.spras) AS LANG_CD,\n    TRIM(T005T.land1) AS CTRY_CD,\n    TRIM(T005T.landx) AS CTRY_SHRT_NM,\n    TRIM(T005T.landx50) AS CTRY_LONG_NM\n  FROM {Config.sourceDatabase}.T005T T005T\n  WHERE T005T._deleted_ = 'F'\n  AND TRIM(T005T.spras) = 'E'\n  AND TRIM(T005T.mandt) = (SELECT MANDT\n                           FROM ref_table\n                           WHERE SRC_SYS_CD='{src_sys_cd}')\n)\n\nSELECT * FROM final_table  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
