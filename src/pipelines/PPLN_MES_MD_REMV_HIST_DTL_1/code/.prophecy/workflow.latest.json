          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(REMOVEHISTORYDETAIL.REMOVEHISTORYDETAILID) as REMV_HIST_DTL_ID\n, TRIM(REMOVEHISTORYDETAIL.PRODUCTID) as PROD_ID\n, INT(REMOVEHISTORYDETAIL.CHANGECOUNT) as CHG_CNT\n, TRIM(REMOVEHISTORYDETAIL.COMPONENTREMOVEHISTORYID) as CMPNT_REMV_HIST_ID\n, TRIM(REMOVEHISTORYDETAIL.DESTINATIONCONTAINERID) as DEST_CNTNR_ID\n, TRIM(REMOVEHISTORYDETAIL.DESTINATIONLOCATIONID) as DEST_LOC_ID\n, TRIM(REMOVEHISTORYDETAIL.DESTINATIONLOT) as DEST_LOT_NUM\n, TRIM(REMOVEHISTORYDETAIL.DESTINATIONSTOCKPOINT) as DEST_STK_PT_VAL\n, CAST(NULL AS STRING) as EXPT_IMPT_KEY_VAL\n, TRIM(REMOVEHISTORYDETAIL.HISTORYID) as HIST_ID\n, TRIM(REMOVEHISTORYDETAIL.ISSUEACTUALSHISTORYID) as ISS_ACTLS_HIST_ID\n, TRIM(REMOVEHISTORYDETAIL.ISSUECONTROL) as ISS_CNTL_CD\n, TRIM(REMOVEHISTORYDETAIL.NOTES) as NOTES_TXT\n, TRIM(REMOVEHISTORYDETAIL.CDOTYPEID) as OBJ_TYPE_ID\n, CAST(REMOVEHISTORYDETAIL.OPENCLOSEDCONTAINER as BOOLEAN ) as OPEN_CLSE_CNTNR_IND\n, TRIM(REMOVEHISTORYDETAIL.REFERENCEDESIGNATOR) as REF_DSGNR_VAL\n, TRIM(REMOVEHISTORYDETAIL.REMOVALREASONID) as RMV_RSN_ID\n, CAST(REMOVEHISTORYDETAIL.REMOVEALLQTY AS DECIMAL(18,4)) as REMV_ALL_QTY\n, TRIM(REMOVEHISTORYDETAIL.REMOVEDIFFERENCEREASONID) as REMV_DIFF_RSN_ID\n, CAST(REMOVEHISTORYDETAIL.QTYREMOVED AS DECIMAL(18,4)) as RMVD_QTY\n, CAST(REMOVEHISTORYDETAIL.QTY2REMOVED AS DECIMAL(18,4)) as RMVD_2_QTY\n, TRIM(REMOVEHISTORYDETAIL.TXNID) as TXN_ID\n, TRIM(REMOVEHISTORYDETAIL.UOMID) as UOM_ID\n, TRIM(REMOVEHISTORYDETAIL.UOM2ID) as UOM_2_ID\nFROM  {Config.sourceDatabase}.REMOVEHISTORYDETAIL as REMOVEHISTORYDETAIL WHERE REMOVEHISTORYDETAIL._deleted_ =  'F'  \n \n\"\"\")",
