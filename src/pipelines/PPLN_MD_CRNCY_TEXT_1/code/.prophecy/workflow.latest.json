          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    -- SQL for the following source systems: bbl,bbn, p01\nWITH ref_table AS\n         ( SELECT '100' AS MANDT, 'bbn' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '100' AS MANDT, 'bbl' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '020' AS MANDT, 'p01' AS SRC_SYS_CD\n         )\nSELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    TCURT.waers AS CRNCY_CD,\n    TCURT.spras AS LANG_CD,\n    TCURT.ktext AS CRNCY_SHRT_NM,\n    TCURT.ltext AS CRCNCY_LONG_NM\nFROM {Config.sourceDatabase}.TCURT TCURT\nWHERE TCURT._deleted_ = 'F'\n  AND TCURT.mandt = ( SELECT MANDT\n                      FROM ref_table\n                      WHERE SRC_SYS_CD='{src_sys_cd}')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
