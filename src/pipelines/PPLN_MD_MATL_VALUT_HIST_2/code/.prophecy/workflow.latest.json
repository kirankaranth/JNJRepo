          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    WITH filters AS\n(\n   SELECT '232' AS `MANDT`, 'geu' AS `SRC_SYS_CD`\n   UNION ALL\n   SELECT '020', 'p01'\n)\nSELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n  MBEWH.matnr AS MATL_NUM,\n  MBEWH.bwkey AS VALUT_AREA_CD,\n  MBEWH.bwtar AS VALUT_TYPE_CD,\n  CAST(MBEWH.lfgja AS INT) AS PSTNG_YR,\n  CAST(MBEWH.lfmon AS INT) AS PSTNG_PER,\n  MBEWH.vprsv AS PRC_CNTL_IND,\n  CAST(MBEWH.lbkum AS DECIMAL(18, 4)) AS TOT_STK_QTY,\n  CAST(MBEWH.salk3 AS DECIMAL(18, 4)) AS TOT_VAL_AMT,\n  CAST(MBEWH.verpr AS DECIMAL(18, 4)) AS MVG_AVG_PRC_AMT,\n  CAST(MBEWH.stprs AS DECIMAL(18, 4)) AS PRC_AMT,\n  CAST(MBEWH.peinh AS DECIMAL(18, 4)) AS PRC_UNIT_NBR,\n  MBEWH.bklas AS VALUT_CLS_CD,\n  MARA.meins AS BASE_UOM_CD\nFROM {Config.sourceDatabase}.MBEWH MBEWH\nLEFT JOIN {Config.sourceDatabase}.MARA MARA ON MBEWH.matnr = MARA.matnr\nAND MARA._deleted_ = 'F'\nAND MARA.mandt = (SELECT MANDT\n                  FROM filters\n                  WHERE SRC_SYS_CD='{src_sys_cd}')\nWHERE MBEWH._deleted_ = 'F'\nAND MBEWH.mandt = (SELECT MANDT\n                  FROM filters\n                  WHERE SRC_SYS_CD='{src_sys_cd}')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
