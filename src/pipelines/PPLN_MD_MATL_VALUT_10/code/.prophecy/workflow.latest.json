          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n  MBEW.matnr AS MATL_NUM,\n  MBEW.bwkey AS VALUT_AREA_CD,\n  MBEW.bwtar AS VALUT_TYPE_CD,\n  MBEW.vprsv AS PRC_CNTL_IND,\n  CAST(MBEW.lbkum AS DECIMAL(18, 4)) AS TOT_STK_QTY,\n  CAST(MBEW.salk3 AS DECIMAL(18, 4)) AS TOT_VAL_AMT,\n  CAST(MBEW.verpr AS DECIMAL(18, 4)) AS MVG_AVG_PRC_AMT,\n  CAST(MBEW.stprs AS DECIMAL(18, 4)) AS PRC_AMT,\n  CAST(MBEW.peinh AS DECIMAL(18, 4)) AS PRC_UNIT_NBR,\n  MBEW.bklas AS VALUT_CLS_CD,\n  MARA.meins AS BASE_UOM_CD,\n  NULL AS DEL_FL,\n  NULL AS MATL_USG,\n  NULL AS MATL_ORIG,\n  NULL AS INHS_PRODCD,\n  NULL AS CST_ELMNT_ORIG_GRP,\n  NULL AS COST_OVHD_GRP,\n  NULL AS VALUT_CLS_DESC,\n  NULL AS TMST\nFROM {Config.sourceDatabase}.MBEW MBEW\nLEFT JOIN {Config.sourceDatabase}.MARA MARA ON MBEW.matnr = MARA.matnr\nAND MARA._deleted_ = 'F' AND MARA.MANDT='400'\nWHERE MBEW._deleted_ = 'F' AND MBEW.MANDT='400'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
