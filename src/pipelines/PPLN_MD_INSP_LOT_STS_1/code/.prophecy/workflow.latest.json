          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    TRIM(QAVE.prueflos) AS LOT_NUM,\n    CAST(QAVE.zaehler AS INT) AS COUNT_NUM,\n    QAVE.vcode AS USG_DCSN_CD,\n    CAST(QAVE.qkennzahl AS INT) AS QUAL_SCORE_NBR,\n    QAVE.vname AS USG_DCSN_NM,\n    IF(QAVE.vaedatum == '00000000', NULL, to_timestamp(CONCAT(QAVE.vaedatum, QAVE.vezeitaen), 'yyyyMMddHHmmss')) AS QC_DCSN_DTTM\nFROM {Config.sourceDatabase}.QAVE QAVE\nWHERE QAVE._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
