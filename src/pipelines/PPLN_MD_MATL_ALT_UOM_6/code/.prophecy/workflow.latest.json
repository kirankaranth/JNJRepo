          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT DISTINCT\n      '{Config.sourceSystem}'   AS SRC_SYS_CD\n    , TRIM(marm.matnr) as MATL_NUM\n    , TRIM(marm.meinh) as ALT_UOM_CD\n    , TRIM(mara.meins) as BASE_UOM_CD\n    , NULL as HARMO_UOM_CD\n    , NULL as STD_UOM_CD\n    , NULL as ENTRP_UOM_CD\n    , cast(trim(marm.umrez) as DECIMAL(18,4)) as LCL_FACT_NUMRTR_MEAS\n    , cast(trim(marm.umren) as DECIMAL(18,4)) as LCL_FACT_DENOM_MEAS\n    , NULL AS BASE_UOM_IND\n    , NULL AS BASE_UOM_MULT_QTY\n    , NULL AS FACT_NUMRTR_MEAS\n    , NULL AS FACT_DENOM_MEAS\n    , TRIM(marm.ean11) as GTIN_NUM\n    , TRIM(marm.numtp) as GTIN_CAT_CD\n    , cast(trim(marm.breit) as DECIMAL(18,4)) as WDTH_MEAS\n    , TRIM(marm.meabm) as DIM_UOM_CD\n    , cast(trim(marm.brgew) as DECIMAL(18,4)) as GRS_WT_MEAS\n    , TRIM(marm.gewei) as WT_UOM_CD\n    , cast(trim(marm.laeng) as DECIMAL(18,4)) as LGTH_MEAS\n    , cast(trim(marm.hoehe) as DECIMAL(18,4)) as HGHT_MEAS\n    , cast(trim(marm.volum) as DECIMAL(18,4)) as VOL_MEAS\n    , TRIM(marm.voleh) as VOL_UOM_CD\n    , cast(TRIM(mara.ntgew)  as DECIMAL(18,4)) as NET_WT_MEAS\nFROM {Config.sourceDatabase}.marm\nLEFT JOIN {Config.sourceDatabase}.mara\n  ON (marm.matnr = mara.matnr\n AND mara._deleted_ = 'F' AND mara.MANDT = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS\n WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter'))\nWHERE marm._deleted_='F' AND  marm.MANDT = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS\n WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter');  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
