          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    select SRC_SYS_CD,MATL_NUM,VALUT_AREA_CD,VALUT_TYPE_CD,PRC_CNTL_IND,TOT_STK_QTY,TOT_VAL_AMT,\nMVG_AVG_PRC_AMT,PRC_AMT,PRC_UNIT_NBR,VALUT_CLS_CD,BASE_UOM_CD,DEL_FL,MATL_USG,MATL_ORIG,INHS_PRODCD,\nCST_ELMNT_ORIG_GRP,COST_OVHD_GRP,VALUT_CLS_DESC,TMST\nfrom(\nSELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n F4105.COLITM AS MATL_NUM,\n    F4105.COMCU AS VALUT_AREA_CD,\n \\\"#\\\" AS VALUT_TYPE_CD,\n F4105.COLEDG AS PRC_CNTL_IND,\n CAST((SUM(CAST(F41021.LIPQOH AS DECIMAL(18,4)))/10000) + (SUM(CAST(F41021.LIQTIN AS DECIMAL(18,4)))/10000) AS DECIMAL(18,4)) AS TOT_STK_QTY,\n    CAST(((CAST(F4105.COUNCS AS DECIMAL(18, 4) )/ 10000) * (SUM(CAST(F41021.LIPQOH AS DECIMAL(18, 4))/10000) + SUM(CAST(F41021.LIQTIN AS DECIMAL(18, 4))/10000)))AS DECIMAL(18, 4)) AS TOT_VAL_AMT,\n NULL AS  MVG_AVG_PRC_AMT,\n CAST(F4105.COUNCS AS DECIMAL(18, 4)) AS PRC_AMT,\n CAST(10000 AS DECIMAL(18, 4)) AS PRC_UNIT_NBR,\n F4101.IMGLPT AS VALUT_CLS_CD,\n F4101.IMUOM1 AS BASE_UOM_CD,\n NULL AS DEL_FL,\n NULL AS MATL_USG,\n NULL AS MATL_ORIG,\n NULL AS INHS_PRODCD,\n NULL AS CST_ELMNT_ORIG_GRP,\n NULL AS COST_OVHD_GRP,\n NULL AS VALUT_CLS_DESC,\n NULL AS TMST,\n ROW_NUMBER() over (partition by F4105.COLITM,F4105.COMCU,\\\"#\\\" order by F4105.COLITM) as RNK\n FROM {Config.sourceDatabase}.F4105 F4105\n LEFT JOIN {Config.sourceDatabase}.F41021 ON F4105.COITM = F41021.LIITM AND F4105.COMCU = F41021.LIMCU AND F41021._deleted_=\\\"F\\\"\n LEFT JOIN {Config.sourceDatabase}.F4101 F4101 ON F4105.COITM = F4101.IMITM AND F4101._deleted_=\\\"F\\\"\n WHERE F4105.COLEDG = '07' AND F4105._deleted_=\\\"F\\\"\n GROUP BY F4105.COLITM,F4105.COMCU,F4105.COLEDG,F4105.COUNCS,F4101.IMGLPT,F4101.IMUOM1) WHERE RNK=1  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
