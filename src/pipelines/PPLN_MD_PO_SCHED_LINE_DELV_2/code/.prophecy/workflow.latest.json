          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\nEKET.ebeln AS PO_NUM,\nEKET.ebelp AS PO_LINE_NBR,\nEKET.etenr AS DELV_SCHED_CNT_NBR,\nCASE\n  WHEN TRIM(EKET.eindt)='00000000' THEN NULL\n  ELSE TO_TIMESTAMP(TRIM(EKET.eindt),\\\"yyyyMMdd\\\")\nEND AS DELV_DTTM,\nCAST(trim(EKET.menge) AS DECIMAL(18,4)) AS SCHD_QTY,\nCAST(trim(EKET.wemng) AS DECIMAL(18,4)) AS RECV_QTY,\nCAST(trim(EKET.glmng) AS DECIMAL(18,4)) AS STK_TFR_RECV_QTY,\nCAST(trim(EKET.dabmg) AS DECIMAL(18,4)) AS MRP_ADJ_QTY,\nCASE WHEN EKET.slfdt = '00000000' THEN NULL\nELSE TO_TIMESTAMP(EKET.slfdt,\\\"yyyyMMdd\\\") END AS STAT_DELV_DTTM,\nCAST(trim(EKET.mng02) AS DECIMAL(18,4)) AS CMT_QTY,\n\\\"#\\\" AS ORDER_TYPE,\n\\\"#\\\" AS ORDER_CO,\n\\\"#\\\" AS ORDER_SUF,\nCAST(trim(EKET.ameng)  AS DECIMAL(18,4)) AS PREV_QTY,\nCASE WHEN EKET.dat01 = '00000000' THEN NULL\nELSE TO_TIMESTAMP(EKET.dat01,\\\"yyyyMMdd\\\") END AS CMT_DTTM,\nTRIM(EKET.lpein) AS CAT_OF_DELV_DT,\nTRIM(EKET.charg) AS BTCH_NUM\nFROM {Config.sourceDatabase}.EKET EKET\nWHERE EKET._deleted_ = 'F'\nAND EKET.mandt = \\\"020\\\"  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
