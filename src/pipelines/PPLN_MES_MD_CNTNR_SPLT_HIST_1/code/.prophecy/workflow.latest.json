          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n   '{Config.sourceSystem}' AS SRC_SYS_CD\n    ,TRIM(SplitHistory.SPLITHISTORYID) as CNTNR_SPLT_HIST_ID\n ,TRIM(SplitHistory.FROMCONTAINERID) as FROM_CNTNR_ID\n ,INT(SplitHistory.CHANGECOUNT) as CHG_CNT\n ,INT(SplitHistory.CHILDCOUNT) as CHILD_CNT\n ,CAST(TRIM(SplitHistory.CLOSED) AS BOOLEAN) as CLSE_IND\n ,TRIM(SplitHistory.EXPORTIMPORTKEY) as EXPT_IMPT_KEY_VAL\n ,TRIM(SplitHistory.HISTORYID) as HIST_ID\n ,TRIM(SplitHistory.HISTORYMAINLINEID) as HIST_MNLINE_ID\n ,CAST(NULL AS BOOLEAN) as IS_TOTLY_CNV_IND\n ,INT(SplitHistory.CDOTYPEID) as OBJ_TYPE_ID\n ,TRIM(SplitHistory.QTY) as SPLT_QTY\n ,TRIM(SplitHistory.QTY2) as SPLT_2_QTY\n ,TRIM(SplitHistory.TXNID) as TRX_ID\n ,CAST(TRIM(SplitHistory.UNITCOUNT) AS INT) as UNIT_CNT\nFROM  {Config.sourceDatabase}.SplitHistory as SplitHistory\nWHERE SplitHistory._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
