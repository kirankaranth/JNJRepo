          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n   '{Config.sourceSystem}' AS SRC_SYS_CD,\ntrim(SplitHistoryDetails.splithistorydetailsid) as CNTNR_SPLT_HIST_DTL_ID,\ntrim(SplitHistoryDetails.splithistoryid) as CNTNR_SPLT_HIST_ID,\ntrim(SplitHistoryDetails.tocontainerid) as TO_CNTNR_ID,\nCAST(trim(SplitHistoryDetails.ADJUSTTHRUPUT) AS BOOLEAN) as ADJ_THRUPUT_IND,\nCAST(SplitHistoryDetails.CHANGECOUNT AS INT) as CHG_CNT,\nCAST(SplitHistoryDetails.CHILDCOUNT AS INT) as CHILD_CNT,\ntrim(SplitHistoryDetails.exportimportkey) as EXPT_IMPT_KEY_VAL,\ntrim(SplitHistoryDetails.historyid) as HIST_ID,\nNULL as LOT_RSN_ID,\nCAST(SplitHistoryDetails.CDOTYPEID AS INT) as OBJ_TYPE_ID,\ntrim(SplitHistoryDetails.qty) as SPLT_QTY,\ntrim(SplitHistoryDetails.qty2) as SPLT_2_QTY,\ntrim(SplitHistoryDetails.txnid) as TRX_ID,\nCAST(SplitHistoryDetails.UNITCOUNT AS INT) as UNIT_CNT\nFROM  {Config.sourceDatabase}.SplitHistoryDetails as SplitHistoryDetails \nWHERE SplitHistoryDetails._deleted_ = 'F'\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
