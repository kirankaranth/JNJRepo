          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\nEBAN.banfn AS PR_NUM,\nCAST (EBAN.bnfpo AS INT) AS PR_LINE_NBR,\nEBAN.bsart AS PR_TYPE_CD,\nEBAN.bstyp AS PR_CAT_CD,\nEBAN.statu AS PR_STS_CD,\nEBAN.ekgrp AS PRCHSNG_GRP_NUM,\nCASE WHEN EBAN.badat= '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE TO_TIMESTAMP(EBAN.badat,'yyyyMMdd') END AS PR_RQST_DTTM,\nEBAN.vrtyp AS PO_TYPE_CD,\nEBAN.lifnr AS ERP_VENDOR_NUMBER,\nEBAN.frgkz AS RELEASE_INDICATOR,\nEBAN.frgzu AS RELEASE_STATUS_CODE,\nEBAN.memory AS PR_NOT_YET_COMPLETE,\nEBAN.wepos AS PR_GOODS_RECEIPT_IND,\nEBAN.repos AS PR_INVOICE_RECEIPT_IND,\nEBAN.twrkz AS PR_PARTIAL_INVOICE_IND,\nCAST (EBAN.rlwrt AS DECIMAL(18,4)) AS PR_RELEASE_VALUE_AMOUNT,\nEBAN.waers AS DOCUMENT_CURRENCY_CODE\nFROM {Config.sourceDatabase}.EBAN EBAN \nWHERE EBAN._deleted_ = 'F' AND  EBAN.MANDT = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
