          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    select\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\n    _sopromet_zzlrp.zzlresn AS RESV_CD,\n    _sopromet_zzlrp.posnr AS RESV_POS,\n    TRIM(_sopromet_zzlrp.matnr) AS MATL_NUM,\n    CAST(TRIM(_sopromet_zzlrp.menge_wa) AS DECIMAL(18,4) ) AS GOODS_ISS_QTY,\n    CAST(TRIM(_sopromet_zzlrp.menge_we) AS DECIMAL(18,4) ) AS GOODS_RECV_QTY,\n    CAST(TRIM(_sopromet_zzlrp.menge_fa) AS DECIMAL(18,4) ) AS BILL_TO_QTY,\n    TRIM(_sopromet_zzlrp.meins) AS BASE_UOM,\n    TRIM(_sopromet_zzlrp.charg) AS BTCH_NUM,\n    CAST(TRIM(_sopromet_zzlrp.menge_nb) AS DECIMAL(18,4) ) AS FRWD_QTY,\n    CAST(TRIM(_sopromet_zzlrp.menge_zf) AS DECIMAL(18,4) ) AS INTERM_DOC_QTY,\n    TRIM(_sopromet_zzlrp.sernr) AS SER_NUM,\n    TRIM(_sopromet_zzlrp.flag_add) AS ADD_FL,\n     CAST(TRIM(_sopromet_zzlrp.menge_mi)  AS DECIMAL(18,4) ) AS MSNG_QTY,\n     CAST(TRIM(_sopromet_zzlrp.menge_dm)  AS DECIMAL(18,4) ) AS DMGD_QTY,\n    TRIM(_sopromet_zzlrp.lresn_ref) AS RESV_REF,\n    TRIM(_sopromet_zzlrp.posnr_ref) AS POS_RESV_REF,\n    TRIM(_sopromet_zzlrp.tray) AS TRAY_CD,\n    TRIM(_sopromet_zzlrp.lgort_wa) AS GOODS_ISS_SLOC,\n    TRIM(_sopromet_zzlrp.werks_wa) AS OTGO_GOODS_WRK,\n    CASE WHEN _sopromet_zzlrp.rdate_bu = '00000000' THEN NULL  ELSE TO_TIMESTAMP(_sopromet_zzlrp.rdate_bu, \\\"yyyyMMdd\\\") END AS ACTL_RTN_DTTM,\n    TRIM(_sopromet_zzlrp.vf_vbeln) AS BILL_ORDR,\n    TRIM(_sopromet_zzlrp.usagecode) AS USG_CD,\n    TRIM(_sopromet_zzlrp.swerk) AS MAINT_PLNT_CD,\n    TRIM(_sopromet_zzlrp.eqart) AS TECH_OBJ_TYPE,\n    TRIM(_sopromet_zzlrp.dcode) AS DMGD_CD,\n    TRIM(_sopromet_zzlrp.kztransfer) AS ITM_XFER,\n    CAST(TRIM(_sopromet_zzlrp.menge_tr)  AS DECIMAL(18,4) ) AS TFR_QTY,\n    TRIM(_sopromet_zzlrp.idnlf) AS SUP_MATL_NUM,\n    TRIM(_sopromet_zzlrp.itm_cust01) AS CUST_DATA_FLD_01,\n    TRIM(_sopromet_zzlrp.sobkz) AS SPL_STK_IN\nFROM {Config.sourceDatabase}._sopromet_zzlrp _sopromet_zzlrp\nWHERE _sopromet_zzlrp._deleted_='F'\nAND _sopromet_zzlrp.mandt = (SELECT param_val from l1_md_prophecy_config.l1_parameters l1_parameters WHERE l1_parameters.env_nm = '{env}' AND l1_parameters.param_nm = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
