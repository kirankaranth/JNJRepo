          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\nTRIM(NSDM_V_MKPF.mblnr) AS MATL_MVMT_NUM,\nTRIM(NSDM_V_MKPF.mjahr) AS MATL_MVMT_YR,\nTRIM(NSDM_V_MKPF.vgart) AS EV_TYPE_CD,\nCASE WHEN NSDM_V_MKPF.bldat= '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE TO_TIMESTAMP(NSDM_V_MKPF.bldat,'yyyyMMdd') END AS MATL_MVMT_DTTM,\nCASE WHEN NSDM_V_MKPF.budat= '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE TO_TIMESTAMP(NSDM_V_MKPF.budat,'yyyyMMdd') END AS PSTNG_DTTM,\nTRIM(NSDM_V_MKPF.bktxt) AS MATL_MVMT_HDR_TXT,\nTRIM(NSDM_V_MKPF.blart) AS DOC_TYPE,\nTRIM(T003T.ltext) AS DOC_TYPE_DESC,\nTRIM(NSDM_V_MKPF.frbnr) AS BILL_OF_LANDG,\nCASE WHEN NSDM_V_MKPF.aedat = '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE TO_TIMESTAMP(NSDM_V_MKPF.aedat,'yyyyMMdd') END AS LAST_CHG_DTTM\nFROM {Config.sourceDatabase}.NSDM_V_MKPF NSDM_V_MKPF\nLEFT JOIN {Config.sourceDatabase}.T003T T003T ON  NSDM_V_MKPF.BLART=T003T.BLART AND T003T._deleted_ = 'F' AND  T003T.MANDT = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter') and T003T.SPRAS=\\\"E\\\"\nWHERE NSDM_V_MKPF._deleted_ = 'F' AND  NSDM_V_MKPF.MANDT = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
