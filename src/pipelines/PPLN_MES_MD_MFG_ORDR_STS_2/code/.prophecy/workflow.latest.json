          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD,\nTRIM(OrderStatus.ORDERSTATUSID) as ORDR_STS_ID,\ncast(TRIM(OrderStatus.ccfCanStartOrder)as BOOLEAN) as CN_STRT_ORDR_IND,\nTRIM(OrderStatus.ccfContainerStatus) as CNTNR_STS_CD,\ncast(TRIM(OrderStatus.CHANGECOUNT)as INT) as CHG_CNT,\nTRIM(OrderStatus.CHANGEHISTORYID) as CHG_HIST_ID,\nTRIM(OrderStatus.ICONID) as ICON_ID,\ncast(TRIM(OrderStatus.ISFROZEN) as BOOLEAN) as IS_FRZN_IND,\nTRIM(OrderStatus.NOTES) as NOTES_TXT,\nNull as ORDR_ST_CD,\nTRIM(OrderStatus.DESCRIPTION) as ORDR_STS_DESC,\ncast(TRIM(OrderStatus.ORDERSTATUSNAME)as int) as ORDR_STS_NM,\nTRIM(OrderStatus.CDOTYPEID) as OBJ_TYPE_ID\nFROM {Config.sourceDatabase}.OrderStatus as OrderStatus\nWhere OrderStatus._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
