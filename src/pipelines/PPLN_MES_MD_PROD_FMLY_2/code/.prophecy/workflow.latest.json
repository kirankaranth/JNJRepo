          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n ,TRIM(ProductFamily.ProductFamilyId) as PROD_FMLY_ID\n ,TRIM(ProductFamily. ProductFamilyName) as PROD_FMLY_NM\n ,TRIM(ProductFamily.CdoTypeId) as OBJ_TYPE_ID\n ,TRIM(ProductFamily.ChangeCount) as CHG_CNT\n ,TRIM(ProductFamily.ChangeStatusId) as CHG_STS_ID\n ,TRIM(ProductFamily.Description) as PROD_FMLY_DESC\n ,TRIM(ProductFamily.Notes) as NOTES_TXT\n ,TRIM(ProductFamily.WorkflowBaseid) as WRKF_BASE_ID\n ,NULL as DVC_TYPE_ID\n ,NULL as PROD_RECIPE_BASE_ID\n ,NULL as PROD_RECIPE_ID\n ,NULL as TRNG_RCNT_EXP_DAYS_CNT\n ,NULL as TRNG_RCNT_ID\n ,CAST(TRIM(ProductFamily.CURRENTCOST) AS DECIMAL(18, 4)) as CUR_COST_AMT\n ,TRIM(ProductFamily.DOCUMENTSETID) as DOC_SET_ID\n ,TRIM(ProductFamily.ICONID) as ICON_ID\n ,CAST(TRIM(ProductFamily.ISFROZEN) AS BOOLEAN) as IS_FRZN_IND\n ,CAST(TRIM(ProductFamily.PLANNEDCOST) AS DECIMAL(18,4)) as PLAN_COST_AMT\n ,TRIM(ProductFamily.SAMPLINGPLANID) as SMPLG_PLAN_ID\n ,CAST(TRIM(ProductFamily.STDCOST) AS DECIMAL(18,4)) as STD_COST_AMT\n ,CAST(ProductFamily.STDSTARTQTY AS DECIMAL(18, 4)) as STD_STRT_QTY\n ,CAST(ProductFamily.STDSTARTQTY2 AS DECIMAL(18, 4)) as STD_STRT_2_QTY\n ,TRIM(ProductFamily.STDSTARTUOM2ID) as STD_STRT_UOM_2_ID\n ,TRIM(ProductFamily.STDSTARTUOMID) as STD_STRT_UOM_ID\n ,TRIM(ProductFamily.TRAININGREQGROUPID) as TRNG_REQ_GRP_ID\n ,TRIM(ProductFamily.WIPMSGDEFMGRID) as WIP_MSG_DEF_MGR_ID\n ,TRIM(ProductFamily.WorkFlowId) as WRKF_ID\n FROM  {Config.sourceDatabase}.ProductFamily as ProductFamily\n WHERE ProductFamily._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
