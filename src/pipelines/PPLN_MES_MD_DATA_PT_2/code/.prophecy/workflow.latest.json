            "script" : "out0 = spark.sql(f\"\"\"\n    select\n  '{Config.sourceSystem}' AS SRC_SYS_CD,\n    trim(DataPoint.datapointid) AS DATA_PT_ID,\n trim(DataPoint.datacollectiondefid) AS DATA_CLCT_DEF_ID,\n trim(DataPoint.attributename) AS ATTR_NM,\n CAST(NULL AS BOOLEAN) AS BOOL_FLSE_IND,\n CAST(NULL AS BOOLEAN) AS BOOL_TR_IND,\n trim(DataPoint.ccfcarryforwarddp) AS CARR_FRWD_DP_NM,\n CAST(DataPoint.CHANGECOUNT AS INT) AS CHG_CNT,\n CAST(DataPoint.COLUMNPOSITION AS INT) AS COL_POS_NBR,\n trim(DataPoint.ccfcontainerfield) AS CNTNR_FLD_NM,\n trim(DataPoint.datapointname) AS DATA_PT_NM,\n trim(DataPoint.datatype) AS DATA_TYPE_CD,\n trim(DataPoint.displaymode) AS DSPLY_MODE_CD,\n NULL AS ETH_CARR_FRWD_DP_NM,\n NULL AS ETH_CNTNR_FLD_NM,\n NULL AS EXPT_IMPT_KEY_VAL,\n CAST(trim(DataPoint.isfrozen) AS BOOLEAN) AS IS_FRZN_IND,\n CAST(trim(DataPoint.islimitoverrideallowed) AS BOOLEAN) AS IS_LMT_OVRD_ALLW_IND,\n CAST(trim(DataPoint.isrequired) AS BOOLEAN) AS IS_REQ_IND,\n trim(DataPoint.lowerlimit) AS LWR_LMT_VAL,\n trim(DataPoint.ccflowerwarninglimit) AS LWR_WARN_LMT_VAL,\n CAST(trim(DataPoint.maptouserattribute) AS BOOLEAN) AS MAP_TO_USER_ATTR_IND,\n trim(DataPoint.ccfnominalvalue) AS NOM_VAL,\n trim(DataPoint.objectgroupid) AS OBJ_GRP_ID,\n trim(DataPoint.objectselvaltype) AS OBJ_SLCT_VTYPE_ID,\n trim(DataPoint.objecttype) AS OBJ_TYPE_CD,\n trim(DataPoint.cdotypeid) AS OBJ_TYPE_ID,\n trim(DataPoint.queryname) AS QRY_NM,\n trim(DataPoint.querytype) AS QRY_TYPE_CD,\n CAST(DataPoint.ROWPOSITION AS INT) AS ROW_POS_NBR,\n CAST(DataPoint.SEQUENCE AS INT) AS SEQ_NBR,\n trim(DataPoint.csctagname) AS TAG_NM,\n trim(DataPoint.uomid) AS UOM_ID,\n trim(DataPoint.upperlimit) AS UP_LMT_VAL,\n trim(DataPoint.ccfupperwarninglimit) AS UP_WARN_LMT_VAL,\n trim(DataPoint.displaylimits) AS DSPLY_LMT_VAL,\n NULL AS RESL_LWR_LMT_VAL,\n NULL AS RESL_LWR_WARN_VAL,\n NULL AS RESL_UP_LMT_VAL,\n NULL AS RESL_UP_WARN_VAL,\n trim(DataPoint.innerlowerlimit) AS INR_LWR_LMT_VAL,\n trim(DataPoint.inn