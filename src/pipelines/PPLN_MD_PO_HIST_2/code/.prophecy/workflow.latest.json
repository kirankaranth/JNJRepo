          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\nEKBE.ebeln AS PO_NUM,\nEKBE.ebelp AS PO_LINE_NBR,\nEKBE.zekkn AS PO_SEQ_NBR,\nEKBE.vgabe AS EV_TYPE_CO,\nCAST(EKBE.gjahr AS INT) AS MATL_MVMT_YR,\nEKBE.belnr AS MATL_MVMT_NUM,\nEKBE.buzei AS MATL_MVMT_SEQ_NBR,\nTRIM(EKBE.bewtp) AS PO_HIST_CAT_CD,\nTRIM(EKBE.bwart) AS MVMT_TYPE_CD,\nCASE WHEN EKBE.budat = '00000000' THEN CAST(NULL AS TIMESTAMP) ELSE TO_TIMESTAMP(EKBE.budat,'yyyyMMdd') END AS PSTNG_DTTM,\nCAST(TRIM(EKBE.menge) AS DECIMAL(18,4)) AS RECV_EA_QTY,\nTRIM(EKBE.matnr) AS ENT_MATL_NUM,\nTRIM(EKBE.werks) AS PLNT_CD,\n'#' AS UNIQ_KEY_ID\nFROM {Config.sourceDatabase}.EKBE EKBE\nWHERE EKBE._deleted_ = 'F'\nAND EKBE.mandt = 100  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
