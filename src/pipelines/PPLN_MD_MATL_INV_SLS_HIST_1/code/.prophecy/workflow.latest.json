          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    WITH filters AS\n(\n   SELECT '232' AS `MANDT`, 'geu' AS `SRC_SYS_CD`\n   UNION ALL\n   SELECT '020', 'p01'\n)\nSELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    MSKAH.matnr AS MATL_NUM,\n    MSKAH.werks AS PLNT_CD,\n    MSKAH.lgort AS SLOC_CD,\n    MSKAH.charg AS BTCH_NUM,\n    MSKAH.sobkz AS SPCL_STCK_IND,\n    MSKAH.vbeln AS SLS_ORDR_NUM,\n    MSKAH.posnr AS SLS_ORDR_LINE_NBR,\n    CAST(MSKAH.lfgja as INT) AS PSTNG_YR,\n    CAST(MSKAH.lfmon as INT) AS PSTNG_PER,\n    CAST(MSKAH.kalab AS DECIMAL(18, 4)) AS UNRSTRCTD_STCK,\n    CAST(MSKAH.kains AS DECIMAL(18, 4)) AS QLTY_INSP_STCK ,\n    CAST(MSKAH.kaspe AS DECIMAL(18, 4)) AS BLCKD_STCK,\n    CAST(MSKAH.kaein AS DECIMAL(18, 4)) AS RSTRCTD_STCK\nFROM {Config.sourceDatabase}.MSKAH MSKAH\nWHERE MSKAH._deleted_ = 'F'\nAND MSKAH.mandt = (SELECT MANDT\n                  FROM filters\n                  WHERE SRC_SYS_CD='{src_sys_cd}')\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
