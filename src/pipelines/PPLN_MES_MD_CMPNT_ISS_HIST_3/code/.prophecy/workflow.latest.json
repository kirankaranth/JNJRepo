          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(COMPONENTISSUEHISTORY.COMPONENTISSUEHISTORYID) as CMPNT_ISS_HIST_ID\n, TRIM(COMPONENTISSUEHISTORY.HISTORYMAINLINEID) as HIST_MNLINE_ID\n, TRIM(COMPONENTISSUEHISTORY.BATCHID) as BTCH_ID\n, CAST(COMPONENTISSUEHISTORY.BILLCONTROL as BOOLEAN ) as BILL_CNTL_IND\n, CAST(COMPONENTISSUEHISTORY.BYPASSEXPIRATIONCHECK as BOOLEAN ) as BYPAS_EXPN_CHK_IND\n, INT(COMPONENTISSUEHISTORY.CHANGECOUNT) as CHG_CNT\n, TRIM(COMPONENTISSUEHISTORY.ccfDeviceId) as DVC_ID\n, CAST(NULL AS STRING) as EXPT_IMPT_KEY_VAL\n, TRIM(COMPONENTISSUEHISTORY.HISTORYID) as HIST_ID\n, TRIM(COMPONENTISSUEHISTORY.MASTERRECIPEID) as MSTR_RECIPE_ID\n, TRIM(COMPONENTISSUEHISTORY.CDOTYPEID) as OBJ_TYPE_ID\n, TRIM(COMPONENTISSUEHISTORY.ccfPCId) as PC_ID\n, TRIM(COMPONENTISSUEHISTORY.RECIPEISSUEITEMID) as RECIPE_ISS_ITM_ID\n, TRIM(COMPONENTISSUEHISTORY.RECIPELISTID) as RECIPE_LIST_ID\n, TRIM(COMPONENTISSUEHISTORY.TXNID) as TXN_ID\nFROM  {Config.sourceDatabase}.COMPONENTISSUEHISTORY as COMPONENTISSUEHISTORY WHERE COMPONENTISSUEHISTORY._deleted_ =  'F'\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
