          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    TRIM(TFACS.ident) AS CAL_NUM,\n    TRIM(TFACS.jahr) AS CAL_YR,\n    TRIM(TFACS.mon01) AS MO1_DAYS_CD,\n    TRIM(TFACS.mon02) AS MO2_DAYS_CD,\n    TRIM(TFACS.mon03) AS MO3_DAYS_CD,\n    TRIM(TFACS.mon04) AS MO4_DAYS_CD,\n    TRIM(TFACS.mon05) AS MO5_DAYS_CD,\n    TRIM(TFACS.mon06) AS MO6_DAYS_CD,\n    TRIM(TFACS.mon07) AS MO7_DAYS_CD,\n    TRIM(TFACS.mon08) AS MO8_DAYS_CD,\n    TRIM(TFACS.mon09) AS MO9_DAYS_CD,\n    TRIM(TFACS.mon10) AS MO10_DAYS_CD,\n    TRIM(TFACS.mon11) AS MO11_DAYS_CD,\n    TRIM(TFACS.mon12) AS MO12_DAYS_CD,\n    TRIM(TFACS.basis) AS FST_WRK_DAY_NBR,\n    TRIM(TFACS.fenum) AS YR_REST_DAYS_CNT,\n    TRIM(TFACS.wenum) AS YR_WRK_DAYS_CNT\nFROM {Config.sourceDatabase}.TFACS TFACS\nWHERE TFACS._deleted_ = 'F'  \n \n\"\"\")",
