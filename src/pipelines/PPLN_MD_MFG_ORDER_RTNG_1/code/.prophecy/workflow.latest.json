          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'  SRC_SYS_CD,\n  AFVC.APLZL AS ORDR_RTNG_CTR_NUM,\n  AFVC.AUFPL AS ORDR_RTNG_NUM,\n  AFVC.ARBID AS WRK_CNTR_ID,\n  AFVC.LTXA1 AS OPER_DESC,\n  NULL AS OPR_STS_CD,\n  NULL AS OPR_STS_TXT,\n  AFVC.STEUS AS OPER_CD,\n  AFVC.UEKAN AS OPT_OVRLAP_IND,\n  AFVC.UEMUS AS REQ_OVRLAP_IND,\n  AFVC.VORNR AS OPER_NUM,\n  AFVU.SLWID AS KEY_PRFL_CD,\n  AFVU.USR00 AS UDF1_TXT,\n  AFVU.USR01 AS UDF2_TXT,\n  AFVU.USR02 AS UDF3_TXT,\n  AFVU.USR03 AS UDF4_TXT,\n  CAST(AFVU.USR04 as decimal(18,4)) AS UDF1_QTY,\n  AFVU.USE04 AS UDF1_UOM_CD,\n  CAST(AFVU.USR05 as decimal(18,4)) AS UDF2_QTY,\n  AFVU.USE05 AS UDF2_UOM_CD,\n  CAST(AFVU.USR06 as decimal(18,4)) AS UDF1_AMT,\n  AFVU.USE06 AS UDF1_CURR_CD,\n  CAST(AFVU.USR07 as decimal(18,4)) AS UDF2_AMT,\n  AFVU.USE07 AS UDF2_CURR_CD,\n  case\n    when usr08 = '00000000' then null\n    else to_timestamp(usr08, 'yyyyMMdd')\n  end as UDF1_DTTM,\n  case\n    when usr09 = '00000000' then null\n    else to_timestamp(USR09, 'yyyyMMdd')\n  end as UDF2_DTTM,\n  AFVU.USR10 AS UDF1_IND,\n  AFVU.USR11 AS UDF2_IND\nFROM\n  {Config.sourceDatabase}.AFVC afvc\n  LEFT JOIN {Config.sourceDatabase}.AFVU afvu ON afvc.AUFPL = afvu.AUFPL\n  AND afvc.aplzl = afvu.aplzl\n  AND AFVU._deleted_ = 'F'\nWHERE\n  AFVC._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
