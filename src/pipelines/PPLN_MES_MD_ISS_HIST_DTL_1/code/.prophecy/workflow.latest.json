          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(ISSUEHISTORYDETAIL.ISSUEHISTORYDETAILID) as ISS_HIST_DTL_ID\n, TRIM(ISSUEHISTORYDETAIL.COMPONENTISSUEHISTORYID) as CMPNT_ISS_HIST_ID\n, TRIM(ISSUEHISTORYDETAIL.ISSUEDIFFERENCEREASONID) as ISS_DIFF_RSN_ID\n, TRIM(ISSUEHISTORYDETAIL.PRODUCTID) as PROD_ID\n, CAST(ISSUEHISTORYDETAIL.ACTUALQTYISSUED AS DECIMAL(18,4)) as ACTL_ISS_QTY\n, CAST(ISSUEHISTORYDETAIL.ACTUALQTY2ISSUED AS DECIMAL(18,4)) as ACTL_ISS_2_QTY\n, CAST(ISSUEHISTORYDETAIL.ALLOWMANUALWEIGHOVERRIDE as BOOLEAN ) as ALLW_MAN_WEIGH_OVRD_IND\n, INT(ISSUEHISTORYDETAIL.CHANGECOUNT) as CHG_CNT\n, CAST(ISSUEHISTORYDETAIL.CLOSEWHENEMPTY as BOOLEAN ) as CLSE_WHEN_EMPTY_IND\n, TRIM(ISSUEHISTORYDETAIL.EXPORTIMPORTKEY) as EXPT_IMPT_KEY_VAL\n, TRIM(ISSUEHISTORYDETAIL.BPFROMCONTAINERCFACTOR) as FROM_CNTNR_CONV_FACT_VAL\n, TRIM(ISSUEHISTORYDETAIL.BPFROMCONTAINERCONVERTEDACTUAL) as FROM_CNTNR_CNV_ACTL_VAL\n, TRIM(ISSUEHISTORYDETAIL.BPFROMCONTAINERUOMID) as FROM_CNTNR_UOM_ID\n, TRIM(ISSUEHISTORYDETAIL.HISTORYID) as HIST_ID\n, TRIM(ISSUEHISTORYDETAIL.BPINTERMEDIATECFACTOR) as INTRM_CONV_FACT_VAL\n, TRIM(ISSUEHISTORYDETAIL.BPINTERMEDIATEID) as INTRM_ID\n, CAST(ISSUEHISTORYDETAIL.BPINTERMEDIATEPLANNEDQTY AS DECIMAL(18,4)) as INTRM_PLAN_QTY\n, TRIM(ISSUEHISTORYDETAIL.BPINTERMEDIATEPLANNEDUOMID) as INTRM_PLAN_UOM_ID\n, CAST(ISSUEHISTORYDETAIL.ISMANUALREADINGONLY as BOOLEAN ) as IS_MAN_READ_ONLY_IND\n, CAST(ISSUEHISTORYDETAIL.BPISQTYADDITIVE as BOOLEAN ) as IS_QTY_ADDTV_IND\n, TRIM(ISSUEHISTORYDETAIL.ISSUECONTROL) as ISS_CNTL_CD\n, CAST(ISSUEHISTORYDETAIL.QTYLOWERLIMIT AS DECIMAL(18,4)) as LWR_LMT_QTY\n, TRIM(ISSUEHISTORYDETAIL.NOTES) as NOTES_TXT\n, TRIM(ISSUEHISTORYDETAIL.CDOTYPEID) as OBJ_TYPE_ID\n, TRIM(ISSUEHISTORYDETAIL.PRODUCTMATERIALLISTITEMID) as PROD_MATL_LIST_ITM_ID\n, TRIM(ISSUEHISTORYDETAIL.REFERENCEDESIGNATOR) as REF_DSGNR_VAL\n, CAST(ISSUEHISTORYDETAIL.QTYREQUIRED AS DECIMAL(18,4)) as REQ_QTY\n, CAST(ISSUEHISTORYDETAIL.QTY2REQUIRED AS DECIMAL(18,4)) as REQ_2_QTY\n, TRIM(ISSUEHISTORYDETAIL.BPTOCONTAINERCFACTOR) as TO_CNTNR_CONV_FACT_VAL\n, TRIM(ISSUEHISTORYDETAIL.BPTOCONTAINERCONVERTEDACTUAL) as TO_CNTNR_CNV_ACTL_VAL\n, TRIM(ISSUEHISTORYDETAIL.BPTOCONTAINERUOMID) as TO_CNTNR_UOM_ID\n, TRIM(ISSUEHISTORYDETAIL.TXNID) as TXN_ID\n, TRIM(ISSUEHISTORYDETAIL.UOMID) as UOM_ID\n, TRIM(ISSUEHISTORYDETAIL.UOM2ID) as UOM_2_ID\n, CAST(ISSUEHISTORYDETAIL.QTYUPPERLIMIT AS DECIMAL(18,4)) as UP_LMT_QTY\nFROM {Config.sourceDatabase}.ISSUEHISTORYDETAIL as ISSUEHISTORYDETAIL WHERE ISSUEHISTORYDETAIL._deleted_ =  'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
