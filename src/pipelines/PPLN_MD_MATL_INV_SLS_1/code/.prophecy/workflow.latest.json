          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    WITH filters AS\n(\n   SELECT '232' AS `MANDT`, 'geu' AS `SRC_SYS_CD`\n   UNION ALL\n   SELECT '020', 'p01'\n)\nSELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    MSKA.matnr AS MATL_NUM,\n    MSKA.werks AS PLNT_CD,\n    MSKA.lgort AS SLOC_CD,\n    MSKA.charg AS BTCH_NUM,\n    MSKA.sobkz AS SPCL_STCK_IND,\n    MSKA.vbeln AS SLS_ORDR_NUM,\n    MSKA.posnr AS SLS_ORDR_LINE_NBR,\n    CAST(MSKA.kalab AS DECIMAL(18, 4)) AS UNRSTRCTD_STCK,\n    CAST(MSKA.kains AS DECIMAL(18, 4)) AS QLTY_INSP_STCK ,\n    CAST(MSKA.kaspe AS DECIMAL(18, 4)) AS BLCKD_STCK,\n    CAST(MSKA.kaein AS DECIMAL(18, 4)) AS RSTRCTD_STCK\nFROM {Config.sourceDatabase}.MSKA MSKA\nWHERE MSKA._deleted_ = 'F'\nAND MSKA.mandt = (SELECT MANDT\n                  FROM filters\n                  WHERE SRC_SYS_CD='{src_sys_cd}')\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
