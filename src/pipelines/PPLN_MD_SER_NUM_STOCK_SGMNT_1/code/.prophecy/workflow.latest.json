          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'  AS SRC_SYS_CD,\n(EQBS.EQUNR) AS EQMNT_NUM,\nTRIM(EQBS.LBBSA) AS STOCK_TYPE_GOODS_MVMT,\nTRIM(EQBS.B_WERK) AS PLNT,\nTRIM(EQBS.B_LAGER) AS STRG_LOC,\nTRIM(EQBS.B_CHARGE) AS BTCH_NUM,\nTRIM(EQBS.SOBKZ) AS SPL_STOCK_IN,\nTRIM(EQBS.KUNNR) AS SPL_STOCK_CUST_ACCT_NUM,\nTRIM(EQBS.LIFNR) AS ACCT_NUM_VEND,\nTRIM(EQBS.KDAUF) AS SLS_ORDR_NUM,\nTRIM(EQBS.KDPOS) AS ITM_NUM_SLS_ORDR,\nTRIM(EQBS.PS_PSP_PNR) AS WBS_ELMNT,\nTRIM(EQBS.DISUB_OWNER) AS OWN_STOCK\nFROM\n    {Config.sourceDatabase}.EQBS EQBS\n    WHERE EQBS._deleted_ = 'F'\n AND  EQBS.MANDT = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS\n WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
