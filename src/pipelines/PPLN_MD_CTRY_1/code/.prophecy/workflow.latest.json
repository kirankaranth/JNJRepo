          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    WITH ref_table AS\n  (SELECT '100' AS `MANDT`,\n          'bbn' AS `SRC_SYS_CD`\n   UNION ALL SELECT '100',\n                    'hcs'\n   UNION ALL SELECT '020',\n                    'p01'),\nfinal_table AS\n(\n    SELECT\n      '{Config.sourceSystem}'   AS SRC_SYS_CD,\n      TRIM(T005.land1) AS CTRY_CD,\n      TRIM(T005T.landx) AS CTRY_SHRT_NM,\n      TRIM(T005T.landx50) AS CTRY_LONG_NM,\n      TRIM(T005.intca) AS ISO_CTRY_CD,\n      TRIM(T005.spras) AS CNTY_LANG_CD,\n      TRIM(T005.kalsm) AS TAX_PCDR_CD,\n      TRIM(T005.waers) AS CTRY_CRNCY_CD,\n      NULL AS CTRY_TYPE_IND\n    FROM {Config.sourceDatabase}.T005T T005T\n    LEFT JOIN {Config.sourceDatabase}.T005 T005 ON T005.land1 = T005T.land1\n    WHERE T005._deleted_ = 'F'\n    AND T005T._deleted_ = 'F'\n    AND TRIM(T005T.spras) = 'E'\n    and T005.MANDT = (SELECT MANDT\n                      FROM ref_table\n                      WHERE SRC_SYS_CD='{src_sys_cd}')\n    and T005T.MANDT = (SELECT MANDT\n                       FROM ref_table\n                       WHERE SRC_SYS_CD='{src_sys_cd}')\n)\n\nSELECT * FROM final_table  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
