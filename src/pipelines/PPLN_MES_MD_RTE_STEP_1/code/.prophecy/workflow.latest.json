            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n     '{Config.sourceSystem}' AS SRC_SYS_CD,\n    trim(RouteStep.routestepid) AS RTE_STEP_ID,\nCAST(RouteStep.changecount AS INT) AS CHG_CNT,\ntrim(RouteStep.isdurationperunit) AS DUR_PER_UNIT_VAL,\ntrim(RouteStep.erpoperation) AS ERP_OPR_TXT,\ntrim(RouteStep.erprouteid) AS ERP_RTE_ID,\ntrim(RouteStep.exportimportkey) AS EXPT_IMPT_KEY_VAL,\ncast(RouteStep.isfrozen AS BOOLEAN) AS IS_FRZN_IND,\ntrim(RouteStep.cdotypeid) AS OBJ_TYPE_ID,\ntrim(RouteStep.description) AS RTE_STEP_DESC,\ntrim(RouteStep.name) AS RTE_STEP_NM,\ntrim(RouteStep.isrunrateoption) AS RUN_RT_OPT_CD,\ncast(RouteStep.sequence AS INT) AS SEQ_NBR,\ntrim(RouteStep.issetuptime) AS SETUP_TIME_VAL,\ntrim(RouteStep.istimeperbatch) AS TIME_PER_BTCH_VAL,\ntrim(RouteStep.isunitsperhour) AS UNIT_PER_HR_VAL\nFROM {Config.sourceDatabase}.RouteStep RouteStep\nWHERE RouteStep._deleted_='F'\n  \n \n\"\"\")",
