          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n   '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(QtyHistory.QTYHISTORYID) as MATL_QTY_HIST_ID\n, TRIM(QtyHistory.BATCHID) as BTCH_ID\n, TRIM(QtyHistory.CARRIERID) as CARR_ID\n, INT(QtyHistory.CHANGECOUNT) as CHG_CNT\n, CAST(QtyHistory.QTY AS DECIMAL(18, 4)) as CHG_QTY\n, CAST(QtyHistory.QTY2 AS DECIMAL(18, 4)) as CHG_2_QTY\n, CAST(QtyHistory.CLOSEWHENZERO AS BOOLEAN) as CLSE_WHEN_ZERO_IND\n, TRIM(QtyHistory.EXPORTIMPORTKEY) as EXPT_IMPT_KEY_VAL\n, TRIM(QtyHistory.HISTORYID) as HIST_ID\n, TRIM(QtyHistory.HISTORYMAINLINEID) as HIST_MNLINE_ID\n, TRIM(QtyHistory.ISCHARGETORESOURCEID) as IS_CHRG_TO_RSRS_ID\n, TRIM(QtyHistory.MASTERRECIPEID) as MSTR_RECIPE_ID\n, TRIM(QtyHistory.CDOTYPEID) as OBJ_TYPE_ID\n, TRIM(QtyHistory.RESOURCEID) as RSRS_ID\n, TRIM(QtyHistory.ROLLUPREASONID) as RLUP_RSN_ID\n, TRIM(QtyHistory.THRUPUTREPORTINGLEVELID) as THRUPUT_RPTG_LVL_ID\n, TRIM(QtyHistory.THRUPUTSUMMARY) as THRUPUT_SUM_TXT\n, TRIM(QtyHistory.TXNID) as TXN_ID\n, CAST(TRIM(QtyHistory.UNITSLOST) AS INT) as UNIT_LOST_NBR\nFROM  {Config.sourceDatabase}.QtyHistory as QtyHistory  WHERE QtyHistory._deleted_ = 'F'\n  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
