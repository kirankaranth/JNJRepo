          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD,\nTRIM(QtyHistoryDetails.QTYHISTORYDETAILSID) as MATL_QTY_HIST_DTL_ID,\nTRIM(QtyHistoryDetails.CONTAINERID) as CNTNR_ID,\nTRIM(QtyHistoryDetails.QTYHISTORYID) as MATL_QTY_HIST_ID,\nTRIM(QtyHistoryDetails.PRODUCTID) as PROD_ID,\ncast(TRIM(QtyHistoryDetails.ADJUSTTHRUPUT)as BOOLEAN) as ADJ_THRUPUT_IND,\nCAST(TRIM(QtyHistoryDetails.CHANGECOUNT)AS INT) as CHG_CNT,\nTRIM(QtyHistoryDetails.CHANGEQTYTYPE) as CHG_QTY_TYPE_CD,\nTRIM(QtyHistoryDetails.CHARGETOSTEPID) as CHRG_TO_STEP_ID,\nCAST (TRIM(QtyHistoryDetails.CHILDRENLOST)AS INT) as CHLD_LOST_NBR,\ncast(TRIM(QtyHistoryDetails.CLOSEWHENZERO)as BOOLEAN) as CLSE_WHEN_ZERO_IND,\nTRIM(QtyHistoryDetails.COMMENTS) as COMT_TXT,\nCAST(TRIM(QtyHistoryDetails.THISCONTAINERCLOSED) AS BOOLEAN) as CNTNR_CLSE_IND,\nCAST(TRIM(QtyHistoryDetails.THISCONTAINERDISASSOCIATED) AS BOOLEAN)as CNTNR_DISASSOCIATED_IND,\nCAST(TRIM(QtyHistoryDetails.COUNTSAGAINSTPRODUCTION) AS BOOLEAN)as CNTS_AGNST_PRDTN_IND,\nNull as EXPT_IMPT_KEY_VAL,\nTRIM(QtyHistoryDetails.HISTORYID) as HIST_ID,\nCAST(TRIM(QtyHistoryDetails.ISROLLUP) AS BOOLEAN)as IS_RLUP_IND,\nCAST(TRIM(QtyHistoryDetails.QTY2) AS DECIMAL(18,4)) as LOST_2_QTY,\nCAST(TRIM(QtyHistoryDetails.QTY) AS DECIMAL(18,4)) as LOST_QTY,\nTRIM(QtyHistoryDetails.CDOTYPEID) as OBJ_TYPE_ID,\nTRIM(QtyHistoryDetails.OPERATIONID) as OPR_ID,\nCAST(TRIM(QtyHistoryDetails.QTYMULTIPLIER) AS INT) as QTY_MULT_NBR,\nTRIM(QtyHistoryDetails.REASONCODEID) as RSN_CD_ID,\nCAST(TRIM(QtyHistoryDetails.RECORDALLQTY) AS BOOLEAN)as REC_ALL_QTY_IND,\nTRIM(QtyHistoryDetails.TXNID) as TXN_ID,\nCAST(TRIM(QtyHistoryDetails.UNITCONTAINER) AS BOOLEAN)as UNIT_CNTNR_IND,\nCAST(TRIM(QtyHistoryDetails.UNITSLOST) AS INT) as UNIT_LOST_NBR,\nTRIM(QtyHistoryDetails.UOMID) as UOM_ID,\nTRIM(QtyHistoryDetails.UOM2ID) as UOM_2_ID\nFROM {Config.sourceDatabase}.QtyHistoryDetails as QtyHistoryDetails\nWhere QtyHistoryDetails._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
