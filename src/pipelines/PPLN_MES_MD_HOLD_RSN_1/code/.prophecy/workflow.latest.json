          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}' AS SRC_SYS_CD,\n  TRIM(HoldReason.HoldReasonId) as HOLD_RSN_ID,\n  cast(TRIM(HoldReason.ChangeCount) as INT) as CHG_CNT,\n  TRIM(HoldReason.ChangeStatusId) as CHG_STS_ID,\n  TRIM(HoldReason.Description) as HOLD_RSN_DESC,\n  TRIM(HoldReason.HoldReasonName) as HOLD_RSN_NM,\n  TRIM(HoldReason.IconId) as ICON_ID,\n  cast(TRIM(HoldReason.IsFrozen) as BOOLEAN) as IS_FRZN_IND,\n  TRIM(HoldReason.Notes) as NOTES_TXT,\n  TRIM(HoldReason.CDOTypeId) as OBJ_TYPE_ID,\n  TRIM(HoldReason.WIPMsgDefMgrId) as WIP_MSG_DEF_MGR_ID\nFROM {Config.sourceDatabase}.HoldReason as HoldReason\nWhere HoldReason._deleted_ = 'F'  \n \n\"\"\")",
