          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'   AS SRC_SYS_CD,\nmseg.mblnr AS MATL_MVMT_NUM,\nmseg.mjahr AS MATL_MVMT_YR,\nmseg.zeile AS MATL_MVMT_ITM_NUM,\nTRIM(mseg.bwart) AS MATL_MVMT_TYPE_CD,\nTRIM(mseg.matnr) AS MATL_NUM,\nTRIM(mseg.werks) AS PLNT_CD,\nTRIM(mseg.lgort) AS SLOC_CD,\nTRIM(mseg.charg) AS BTCH_NUM,\nTRIM(mseg.insmk) AS STK_TYPE_CD,\nTRIM(mseg.sobkz) AS SPL_STK_TYPE_CD,\nTRIM(mseg.lifnr) AS SUPPLIER_NUM,\nTRIM(mseg.kunnr) AS CUST_NUM,\nTRIM(mseg.kdauf) AS SLS_ORDR_NUM,\nTRIM(mseg.kdpos) AS SLS_ORDR_LINE_NBR,\nTRIM(mseg.shkzg) AS DR_CR_IND,\nCAST(trim(mseg.menge) AS DECIMAL(18,4)) AS ITM_QTY,\nTRIM(mseg.meins)  AS BASE_UOM_CD,\nCAST(trim(mseg.erfmg)  AS DECIMAL(18,4)) AS ENT_UOM_QTY,\nTRIM(mseg.erfme) AS ENT_UOM_CD,\nTRIM(mseg.ebeln) AS PO_NUM,\nTRIM(mseg.ebelp) AS PO_LINE_NBR,\nTRIM(mseg.sgtxt) AS MATERIAL_MVMT_ITM_TXT,\nTRIM(mseg.aufnr) AS MFG_ORDR_NUM,\nNULL AS DELV_NUM,\nNULL AS DELV_LINE_NBR,\nTRIM(mseg.elikz) AS GR_STATUS_CODE,\nTRIM(mseg.line_id) AS GR_LINE_ID,\nTRIM(mseg.ematn) AS EM_MATERIAL_CODE,\nCAST (TRIM (mseg.bstmg) AS decimal(18,4)) AS GR_LINE_QUANTITY,\nTRIM(mseg.bprme) AS GR_PRICE_UOM,\nCAST (TRIM (mseg.bpmng) AS decimal(18,4)) AS GR_LINE_QTY_IN_PRICE_UOM,\nTRIM(mseg.bukrs) AS LEGAL_ENTITY_CODE,\nTRIM(mseg.waers) AS GR_CURRENCY_CODE,\nTRIM(mseg.zustd) AS BTCH_IN_RSTK_USE_STK,\nTRIM(mseg.zusch) AS BTCH_STS_KEY,\nCAST (TRIM (mseg.dmbtr) AS decimal(18,2)) AS GR_LINE_SPEND_AMOUNT,\nTRIM (mseg.lgnum) AS WHSE_NUM,\ncase when mseg.budat_mkpf = '00000000' then null\nelse to_timestamp(mseg.budat_mkpf, \\\"yyyyMMdd\\\") end as PSTNG_DTTM,\nTRIM(mseg.tcode2_mkpf)AS TRX_CD,\nTRIM(mseg.usnam_mkpf) AS USER_NM,\nTRIM(t156ht.btext) AS MVMT_TYPE_TEXT,\nTRIM(mseg.xblnr_mkpf) AS REF_DOC_NUM,\nTRIM(mseg.zekkn) AS SEQ_NUM_OF_ACCT_ASGNMT,\nTRIM(mseg.bstme) AS PRCH_ORDR_UNIT_OF_MEAS,\ntrim(mseg.mat_kdauf) AS SLS_ORDR_NUM_OF_SLS_ORDR_STK,\ntrim(mseg.mat_kdpos) as SLS_ORDR_ITM_OF_SLS_ORDR_STK,\nNULL AS RSN_FOR_MVMT,\nNULL AS PRFT_CTR,\ncast(NULL as timestamp) AS ACTG_DOC_ENT_DTTM,\ncast(NULL as timestamp) AS PSTNG_DTTM_2,\n'#' as SPLT_GUID_PART1,\n'#' as SPLT_GUID_PART2,\n'#' as SPLT_GUID_PART3,\n'#' as SPLT_GUID_PART4,\n'#' as SPLT_GUID_PART5,\n'#' as SPLT_GUID_PART6\nFROM {Config.sourceDatabase}.mseg mseg\nleft join {Config.sourceDatabase}.t156ht on  mseg.bwart = t156ht.bwart\nand t156ht.spras='E'   AND t156ht.MANDT=\\\"800\\\" AND t156ht._deleted_ = 'F'\nWHERE mseg._deleted_ = 'F' AND mseg.MANDT=\\\"800\\\"  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
