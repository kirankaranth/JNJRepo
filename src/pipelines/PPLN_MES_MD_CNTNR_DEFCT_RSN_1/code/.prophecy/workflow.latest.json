          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(CONTAINERDEFECTREASON.DefectReasonId) as CNTNR_DEFCT_RSN_ID\n, INT(CONTAINERDEFECTREASON.ChangeCount) as CHG_CNT\n, TRIM(CONTAINERDEFECTREASON.ChangeHistoryId) as CHG_HIST_ID\n, TRIM(CONTAINERDEFECTREASON.DefectReasonName) as CNTNR_DEFCT_RSN_NM\n, TRIM(CONTAINERDEFECTREASON.Description) as CNTNR_DEFCT_RSN_DESC\n, TRIM(CONTAINERDEFECTREASON.IconId) as ICON_ID\n, CAST(CONTAINERDEFECTREASON.IsFrozen as BOOLEAN ) as IS_FRZN_IND\n, TRIM(CONTAINERDEFECTREASON.Notes) as NOTES_TXT\n, TRIM(CONTAINERDEFECTREASON.CDOTypeId) as OBJ_TYPE_ID\nFROM {Config.sourceDatabase}.CONTAINERDEFECTREASON as CONTAINERDEFECTREASON\nWHERE CONTAINERDEFECTREASON._deleted_ =  'F'  \n \n\"\"\")",
