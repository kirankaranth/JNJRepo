          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'   AS SRC_SYS_CD,\nEQST.EQUNR AS EQMNT_NUM,\nEQST.WERKS AS PLNT,\nEQST.STLAN AS BOM_USG,\nEQST.STLNR AS BOM,\nEQST.STLAL AS ALT_BOM,\ncase when EQST.ANDAT = '00000000' then null else to_timestamp(EQST.ANDAT,\\\"yyyyMMdd\\\") end as REC_CRT_ON_DTTM ,\nTRIM(EQST.ANNAM) AS USER_CRT_REC,\ncase when EQST.AEDAT = '00000000' then null else to_timestamp(EQST.AEDAT,\\\"yyyyMMdd\\\") end as LAST_CHG_ON_DTTM ,\nTRIM(EQST.AENAM) AS NM_PRSN_CHG_OBJ\nFROM {Config.sourceDatabase}.EQST EQST\nWHERE EQST._deleted_ = 'F'\nAND EQST.mandt = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}'\nAND L1_PARAMETERS.PARAM_NM = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
