          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\nF0401.A6AN8 AS SUP_NUM,\nNULL AS PRCHSNG_ORG_NUM,\nCASE WHEN LOWER(TRIM(a6upmj)) = 'null' OR TRIM(a6upmj) = '' OR TRIM(a6upmj) = '0' THEN NULL ELSE to_timestamp(substr(CAST(date_add(concat(substr(CAST(CAST(TRIM(a6upmj) AS INT) + 1900000 AS STRING),1,4),'-01-01'), CAST(substr(CAST(CAST(TRIM(a6upmj) AS INT) + 1900000 AS string),5) AS INT )-1) AS STRING), 1, 10),'yyyy-MM-dd') END AS CRT_ON_DTTM,\nNULL AS PRCH_BLK_IND,\nNULL AS DEL_IND,\nTRIM(F0401.A6CRRP) AS CRNCY_CD,\nNULL AS PMT_TERM_CD,\nTRIM(F0401.A6FRTH) AS INCOTERM1_CD,\nF0101.ABMCU AS INCOTERM2_CD,\nNULL AS PRC_PCDR_CD,\nNULL AS PRC_CNTL_CD,\nTRIM(F0401.A6AVCH) AS EVAL_RCPT_SETLM_CD,\nNULL AS RTRN_VEND_IND,\nNULL AS CNFRM_CD,\nNULL AS NM_OF_PRSN_RESP_CREAT_OBJ,\nNULL AS GR_BAS_INVC_VERIF,\nNULL AS AUTO_GNR_OF_PO_ALLW,\nNULL AS AUTO_EVAL_RCPT_SETLM,\nNULL AS OWN_EXPLN_OF_TERM_OF_PMT,\nNULL AS SHIPPING_COND_CD\nFROM {Config.sourceDatabase}.F0401 F0401\nLEFT JOIN {Config.sourceDatabase}.F0101 F0101 ON F0401.A6AN8=F0101.ABAN8 AND F0101._deleted_ = 'F'\nWHERE F0401._deleted_ = 'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
