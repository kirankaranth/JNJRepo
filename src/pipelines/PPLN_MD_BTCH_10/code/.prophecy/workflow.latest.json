          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    select \nSRC_SYS_CD,SRC_TBL_NM,MATL_NUM,BTCH_NUM,PLNT_CD,DEL_IND,CRT_DTTM,CHG_DTTM,AVAIL_DTTM,BTCH_EXP_DTTM,BTCH_STS_CD,BTCH_LAST_STS_DTTM,SUP_NUM,SUP_BTCH_NUM,BTCH_LAST_GR_DTTM,BTCH_MFG_DTTM,BTCH_TYPE,SUI_IND,LOT_GRADE,PARENT_CODE,SHRT_MATL_NUM from \n(select *, ROW_NUMBER() over (partition by MATL_NUM,BTCH_NUM,PLNT_CD,SHRT_MATL_NUM order by CHG_DTTM) as RNK FROM\n(select\n'{Config.sourceSystem}'   AS SRC_SYS_CD,\n'f4108_adt'      AS SRC_TBL_NM,\nf4108_adt.iolitm AS MATL_NUM,\nf4108_adt.iolotn AS BTCH_NUM,\nf4108_adt.iomcu  AS PLNT_CD,\nNULL as DEL_IND,\n\nto_timestamp(date_add(cast(substring(ioohdj + 1900000,1,4) AS Date),cast(substring(ioohdj,4,3) as int)-1))  AS CRT_DTTM,\n\n\nto_timestamp(date_add(cast(substring(ioupmj + 1900000,1,4) AS Date),cast(substring(ioupmj,4,3) as int)-1)) AS CHG_DTTM,\n\n\nto_timestamp(date_add(cast(substring(iodlej + 1900000,1,4) AS Date),cast(substring(iodlej,4,3) as int)-1)) AS AVAIL_DTTM,\n\n\nto_timestamp(date_add(cast(substring(iommej + 1900000,1,4) AS Date),cast(substring(iommej,4,3) as int)-1)) AS BTCH_EXP_DTTM,\n\ntrim(f4108_adt.iolots) as BTCH_STS_CD,\nNULL as BTCH_LAST_STS_DTTM,\ntrim(f4108_adt.iovend) as SUP_NUM,\ntrim(f4108_adt.iorlot) as SUP_BTCH_NUM,\nNULL as BTCH_LAST_GR_DTTM,\nNUll as BTCH_MFG_DTTM,\ntrim(f4108_adt.iodcto) as BTCH_TYPE,\ntrim(f4108_adt.iochdf) as SUI_IND,\ntrim(f4108_adt.iolotg) as LOT_GRADE,\nNULL as PARENT_CODE,\nf4108_adt.ioitm as SHRT_MATL_NUM\nfrom {Config.sourceDatabase}.f4108_adt\nWHERE f4108_adt._deleted_='F'))\nwhere RNK =1  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
