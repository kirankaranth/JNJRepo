          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'  AS SRC_SYS_CD,\n MBEW.MATNR AS MATL_NUM,\n MBEW.BWKEY AS VALUT_AREA_CD,\n MBEW.BWTAR AS VALUT_TYPE_CD,\n TRIM(MBEW.VPRSV) AS PRC_CNTL_IND,\n CAST(TRIM(MBEW.LBKUM) AS DECIMAL(18,4)) AS TOT_STK_QTY,\n CAST(TRIM(MBEW.SALK3) AS DECIMAL(18,4)) AS TOT_VAL_AMT,\n CAST(TRIM(MBEW.VERPR) AS DECIMAL(18,4)) AS MVG_AVG_PRC_AMT,\n CAST(TRIM(MBEW.STPRS) AS DECIMAL(18,4)) AS PRC_AMT,\n CAST(TRIM(MBEW.PEINH) AS DECIMAL(18,4)) AS PRC_UNIT_NBR,\n TRIM(MBEW.BKLAS) AS VALUT_CLS_CD,\n TRIM(MARA.MEINS) AS BASE_UOM_CD,\n TRIM(MBEW.LVORM) AS DEL_FL,\n TRIM(MBEW.MTUSE) AS MATL_USG,\n TRIM(MBEW.MTORG) AS MATL_ORIG,\n TRIM(MBEW.OWNPR) AS INHS_PRODCD,\n TRIM(MBEW.HRKFT) AS CST_ELMNT_ORIG_GRP,\n TRIM(MBEW.KOSGR) AS COST_OVHD_GRP,\n NULL AS VALUT_CLS_DESC,\n CASE WHEN MBEW.TIMESTAMP = '0' THEN null ELSE to_timestamp(MBEW.TIMESTAMP,\\\"yyyyMMddHHmmss\\\") END AS TMST\n FROM {Config.sourceDatabase}.MBEW MBEW\n LEFT JOIN {Config.sourceDatabase}.MARA on MBEW.MATNR=MARA.MATNR AND MARA._deleted_=\\\"F\\\" AND  MARA.MANDT=(SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')\n where \n MBEW._deleted_=\\\"F\\\" AND  MBEW.MANDT=(SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
        }
