            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}' AS SRC_SYS_CD\n ,trim(DataCollectionDef.DATACOLLECTIONDEFID) AS DATA_CLCT_DEF_ID\n ,trim(DataCollectionDef.datacollectiondefbaseid) AS DATA_CLCT_DEF_BASE_ID\n ,CAST(DataCollectionDef.CHANGECOUNT AS INT) AS CHG_CNT\n ,trim(DataCollectionDef.CHANGEHISTORYID) AS CHG_HIST_ID\n ,NULL AS DVC_TYPE_ID\n ,CAST(NULL AS BOOLEAN) AS DISA_FOR_USER_ENT_IND\n ,trim(DataCollectionDef.description) AS DATA_CLCT_DEF_DESC\n ,trim(DataCollectionDef.datacollectiondefrevision) AS DATA_CLCT_DEF_RVSN_ID\n ,trim(DataCollectionDef.datapointlayout) AS DATA_PT_LYOUT_CD\n ,trim(DataCollectionDef.DISPLAYLIMITS) AS DSPLY_LMT_TXT\n ,trim(DataCollectionDef.eco) AS ENG_CHG_ORDR_NUM\n ,NULL AS FAIL_MODE_ID\n ,trim(DataCollectionDef.iconid) AS ICON_ID\n ,trim(DataCollectionDef.instructions) AS INSTR_TXT\n ,CAST(trim(DataCollectionDef.isfrozen) AS BOOLEAN) AS IS_FRZN_IND\n ,CAST(NULL AS BOOLEAN) AS IS_RSRS_DC_IND\n ,NULL AS MACH_IO_RSRS_GRP_ID\n ,NULL AS MACH_IO_TYPE_CD\n ,NULL AS MACH_READ_TRIGR_TAG_CD\n ,NULL AS MACH_READ_TRIGR_VAL\n ,NULL AS MACH_READ_WAIT_DELAY_VAL\n ,NULL AS NCR_FAIL_CD\n ,trim(DataCollectionDef.notes) AS NOTES_TXT\n ,trim(DataCollectionDef.cdotypeid) AS OBJ_TYPE_ID\n ,trim(DataCollectionDef.parametricdatacontrol) AS PARAMETRIC_DATA_DEF_TYPE_CD\n ,trim(DataCollectionDef.parametricdatadeftype) AS PARM_IC_DATA_DEF_TYPE_CD\n ,NULL AS RSRS_TYPE_ID\n ,NULL AS SC_MACH_READ_TRIGR_TAG_NM\n    ,trim(DataCollectionDef.spcchartdefgroupid) AS SPC_CHRT_DEF_GRP_ID\n ,NULL AS SPC_CHRT_GRP_ID\n ,trim(DataCollectionDef.status) AS STS_CD\n ,trim(DataCollectionDef.webpartid) AS WEB_PART_ID\n ,trim(DataCollectionDef.wipmsgdefmgrid) AS WIP_MSG_DEF_MGR_ID\nFROM {Config.sourceDatabase}.DataCollectionDef DataCollectionDef\nWHERE DataCollectionDef._deleted_='F'  \n \n\"\"\")",
