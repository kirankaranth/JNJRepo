          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT DISTINCT\n      '{Config.sourceSystem}'   AS SRC_SYS_CD\n    , TRIM(f4101.imlitm) as MATL_NUM\n    , TRIM(f41002.umum) as ALT_UOM_CD\n    , NULL as BASE_UOM_CD\n    , NULL as HARMO_UOM_CD\n    , NULL as STD_UOM_CD\n    , NULL as ENTRP_UOM_CD\n    , cast(NULL as DECIMAL(18,4)) as LCL_FACT_NUMRTR_MEAS\n    , cast(NULL as DECIMAL(18,4)) as LCL_FACT_DENOM_MEAS\n    , NULL as BASE_UOM_IND\n    , cast(NULL as DECIMAL(18,4)) as BASE_UOM_MULT_QTY\n    , cast(f41002.umconv/100000000 as DECIMAL(18,4)) as FACT_NUMRTR_MEAS\n    , cast(NULL as DECIMAL(18,4)) as FACT_DENOM_MEAS\n    , NULL as GTIN_NUM\n    , NULL as GTIN_CAT_CD\n    , cast(NULL as DECIMAL(18,4)) as WDTH_MEAS\n    , NULL as DIM_UOM_CD\n    , cast(NULL as DECIMAL(18,4)) as GRS_WT_MEAS\n    , TRIM(f4101.imuwum) as WT_UOM_CD\n    , cast(NULL as DECIMAL(18,4)) as LGTH_MEAS\n    , cast(NULL as DECIMAL(18,4)) as HGHT_MEAS\n    , cast(NULL as DECIMAL(18,4)) as VOL_MEAS\n    , TRIM(f4101.imuvm1) as VOL_UOM_CD\n    , cast(NULL as DECIMAL(18,4)) as NET_WT_MEAS\nFROM {Config.sourceDatabase}.f4101\nLEFT JOIN {Config.sourceDatabase}.f41002\n  ON TRIM(f4101.imitm) = TRIM(f41002.umitm)\n AND TRIM(f4101.imuom1) = TRIM(f41002.umrum)\n AND f41002._deleted_='F'\nWHERE f4101._deleted_='F'  \n \n\"\"\")",
