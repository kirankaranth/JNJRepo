          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(MOVEINHISTORY.MoveInHistoryId) as MATL_MOVE_IN_HIST_ID\n, TRIM(MOVEINHISTORY.HistoryMainlineId) as HIST_MNLINE_ID\n, INT(MOVEINHISTORY.ChangeCount) as CHG_CNT\n, INT(MOVEINHISTORY.ChildCount) as CHILD_CNT\n, TRIM(MOVEINHISTORY.CycleTime) as CYC_TIME_VAL\n, TRIM(MOVEINHISTORY.ElapsedTime) as ELAP_TIME_VAL\n, TRIM(MOVEINHISTORY.HistoryId) as HIST_ID\n, TRIM(MOVEINHISTORY.LocationId) as LOC_ID\n, TRIM(MOVEINHISTORY.CDOTypeId) as OBJ_TYPE_ID\n, TRIM(MOVEINHISTORY.OwnerId) as OWN_ID\n, TRIM(MOVEINHISTORY.ProductId) as PROD_ID\n, CAST(MOVEINHISTORY.Qty AS DECIMAL(18,4)) as MOVE_QTY\n, CAST(MOVEINHISTORY.Qty2 AS DECIMAL(18,4)) as MOVE_2_QTY\n, TRIM(MOVEINHISTORY.Run) as RUN_IND\n, TRIM(MOVEINHISTORY.SetupId) as SETUP_ID\n, TRIM(MOVEINHISTORY.TxnId) as TXN_ID\n, INT(MOVEINHISTORY.UnitCount) as UNIT_CNT\n, TRIM(MOVEINHISTORY.UOM2Id) as UOM_2_ID\n, TRIM(MOVEINHISTORY.UOMId) as UOM_ID\nFROM  {Config.sourceDatabase}.MOVEINHISTORY as MOVEINHISTORY\nWHERE MOVEINHISTORY._deleted_ =  'F'  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
