          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(CDODEFINITION.CDODEFID) as OBJ_TYPE_ID\n, TRIM(CDODEFINITION.DEFAULTTABLEID) as DFLT_TBL_ID\n, TRIM(CDODEFINITION.DISPLAYNAMELABELID) as DSPLY_NM_LBL_ID\n, CAST(CDODEFINITION.ENFORCEINTEGRITY as BOOLEAN ) as ENFRC_INTEG_IND\n, TRIM(CDODEFINITION.FEATUREID) as FEAT_ID\n, TRIM(CDODEFINITION.INHERITMASK) as INHERIT_MASK_CD\n, CAST(CDODEFINITION.ISABSTRACT as BOOLEAN ) as IS_ABSTR_IND\n, CAST(CDODEFINITION.ISEXPOSEDTOCLIENTUI as BOOLEAN ) as IS_EXPO_TO_CLNT_UI_IND\n, CAST(CDODEFINITION.ISWSEXPOSEDSTD as BOOLEAN ) as IS_WS_EXPO_STD_IND\n, TRIM(CDODEFINITION.MAINTENANCETYPEID) as MAINT_TYPE_ID\n, TRIM(CDODEFINITION.CDODESCRIPTION) as OBJ_DESC\n, TRIM(CDODEFINITION.CDONAME) as OBJ_NM\n, TRIM(CDODEFINITION.CDOUSAGEMASK) as OBJ_USG_MASK_CD\n, TRIM(CDODEFINITION.OBJECTSTOCACHE) as OBJS_TO_CACHE_TXT\n, TRIM(CDODEFINITION.PARENTCDOID) as PARNT_OBJ_TYPE_ID\n, CAST(CDODEFINITION.READOVERRIDE as BOOLEAN ) as READ_OVRD_IND\n, TRIM(CDODEFINITION.REVISIONTYPEID) as RVSN_TYPE_ID\n, TRIM(CDODEFINITION.SECURITYTYPEID) as SCTY_TYPE_ID\n, TRIM(CDODEFINITION.SELVALQUERYDEFID) as SEL_VAL_QRY_DEF_ID\n, TRIM(CDODEFINITION.STORAGECATEGORYID) as STRG_CAT_ID\n, TRIM(CDODEFINITION.UIDETAILSID) as UI_DTLS_ID\n, CAST(CDODEFINITION.USEINSTANCESECURITY as BOOLEAN ) as USE_INST_SCTY_IND\n, TRIM(CDODEFINITION.VISIBILITY) as VIS_CD\n, TRIM(CDODEFINITION.WORKSPACECODE) as WRKSPC_CD\n, CAST(CDODEFINITION.WRITEOVERRIDE as BOOLEAN ) as WRITE_OVRD_IND\nFROM  {Config.sourceDatabase}.CDODEFINITION as CDODEFINITION WHERE CDODEFINITION._deleted_ =  'F'\n  \n \n\"\"\")",
