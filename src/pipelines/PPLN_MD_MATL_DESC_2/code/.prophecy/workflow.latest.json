          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n  '{Config.sourceSystem}'  SRC_SYS_CD,\n    TRIM(MAKT.MATNR) AS MATL_NUM,\n    TRIM(MAKT.SPRAS) AS LANG_CD,\n    TRIM(MAKT.MAKTX) AS MATL_SHRT_DESC,\n    TRIM(MAKT.MAKTG) AS MATL_SHRT_DESC_UP_CASE\nFROM\n  {Config.sourceDatabase}.MAKT MAKT\nWHERE\n  MAKT._deleted_ = 'F' and\n  MAKT.MANDT = (SELECT L1_PARAMETERS.PARAM_VAL from l1_md_prophecy_config.l1_parameters L1_PARAMETERS\n WHERE L1_PARAMETERS.ENV_NM = '{env}' AND L1_PARAMETERS.PARAM_NM = 'mandt_filter')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
