          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n    '{Config.sourceSystem}'   AS SRC_SYS_CD,\n    MSKAH.matnr AS MATL_NUM,\n    MSKAH.werks AS PLNT_CD,\n    MSKAH.lgort AS SLOC_CD,\n    MSKAH.charg AS BTCH_NUM,\n    MSKAH.sobkz AS SPCL_STCK_IND,\n    MSKAH.vbeln AS SLS_ORDR_NUM,\n    MSKAH.posnr AS SLS_ORDR_LINE_NBR,\n    cast(MSKAH.lfgja as INT) AS PSTNG_YR,\n    cast(MSKAH.lfmon as INT) AS PSTNG_PER,\n    CAST(MSKAH.kalab AS DECIMAL(18, 4)) AS UNRSTRCTD_STCK,\n    CAST(MSKAH.kains AS DECIMAL(18, 4)) AS QLTY_INSP_STCK ,\n    CAST(MSKAH.kaspe AS DECIMAL(18, 4)) AS BLCKD_STCK,\n    CAST(MSKAH.kaein AS DECIMAL(18, 4)) AS RSTRCTD_STCK\nFROM {Config.sourceDatabase}.MSKAH MSKAH\nWHERE MSKAH._deleted_ = 'F'  \n \n\"\"\")",
