          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    WITH ref_table AS\n         ( SELECT '100' AS MANDT, 'bbn' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '100' AS MANDT, 'bbl' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '100' AS MANDT, 'mrs' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '020' AS MANDT, 'p01' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '600' AS MANDT, 'mbp' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '400' AS MANDT, 'bwi' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '050' AS MANDT, 'svs' AS SRC_SYS_CD\n           UNION ALL\n           SELECT '100' AS MANDT, 'atl' AS SRC_SYS_CD\n         )\nSELECT\n    (\n        SELECT\n            src_sys.src_sys_cd\n        FROM\n            l1.src_sys\n        WHERE\n                src_sys.src_sys_cd = '{src_sys_cd}'\n          AND src_sys.active = TRUE\n    ) AS SRC_SYS_CD,\n    TCURT.waers AS CRNCY_CD,\n    TCURT.spras AS LANG_CD,\n    TRIM(TCURT.ktext) AS CRNCY_SHRT_NM,\n    TRIM(TCURT.ltext) AS CRCNCY_LONG_NM\nFROM {Config.sourceDatabase}.TCURT TCURT\nWHERE TCURT._deleted_ = 'F'\n  AND TCURT.mandt = ( SELECT MANDT\n                      FROM ref_table\n                      WHERE SRC_SYS_CD='{src_sys_cd}')  \n \n\"\"\")",
            "scriptMethodHeader" : "def Script(spark: SparkSession) -> DataFrame:",
            "scriptMethodFooter" : "    return out0"
          }
