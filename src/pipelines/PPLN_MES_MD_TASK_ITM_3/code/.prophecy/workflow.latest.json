          "properties" : {
            "script" : "out0 = spark.sql(f\"\"\"\n    SELECT\n   '{Config.sourceSystem}' AS SRC_SYS_CD\n, TRIM(TASKITEM.TASKITEMID) as TASK_ITM_ID\n, TRIM(TASKITEM.TASKLISTID) as TASKLIST_ID\n, CAST(TASKITEM.ADVANCETONEXTTASK AS BOOLEAN) as ADVN_TO_NEXT_TASK_IND\n, NULL as ALLW_MAN_WT_ENT_OVRD_IND\n, INT(TASKITEM.CHANGECOUNT) as CHG_CNT\n, TRIM(TASKITEM.COMPUTATIONID) as CMPUT_ID\n, TRIM(TASKITEM.DATACOLLECTIONDEFBASEID) as DATA_CLCT_DEF_BASE_ID\n, TRIM(TASKITEM.DATACOLLECTIONDEFID) as DATA_CLCT_DEF_ID\n, TRIM(TASKITEM.DOCUMENTSETID) as DOC_SET_ID\n, TRIM(TASKITEM.ESIGREQUIREMENTID) as ESIG_REQ_ID\n, TRIM(TASKITEM.EXPORTIMPORTKEY) as EXPT_IMPT_KEY_VAL\n, NULL as EXPRSSN_DPNDS_ON_FROM_CNTNR_IND\n, NULL as FAIL_MODE_ID\n, TRIM(TASKITEM.INSTRUCTION) as INSTR_TXT\n, TRIM(TASKITEM.INSTRUCTIONTYPE) as INSTR_TYPE_CD\n, CAST(TASKITEM.ISFROZEN AS BOOLEAN) as IS_FRZN_IND\n, NULL as IS_TLRNC_OVRD_ALLW_IND\n, NULL as MAN_WT_ENT_IND\n, NULL as MANUL_WT_OVRD_ESIGN_ID\n, NULL as MATCH_BY_PROD_NM_IND\n, NULL as MATL_FROM_SAME_CNTNR_IND\n, INT(TASKITEM.MAXITERATIONS) as MAX_ITERS_CNT\n, NULL as MAX_QTY_ALLW_EXPRSSN_TXT\n, INT(TASKITEM.MINITERATIONS) as MIN_ITERS_CNT\n, NULL as MIN_QTY_ALLW_EXPRSSN_TXT\n, NULL as NCR_FAIL_CD\n, TRIM(TASKITEM.CDOTYPEID) as OBJ_TYPE_ID\n, NULL as PROD_BASE_ID\n, NULL as PROD_ID\n, NULL as QTY_ADDTV_IND\n, TRIM(TASKITEM.REPORTINSTRUCTION) as RPT_INSTR_TXT\n, NULL as REQ_QTY_EXPRSSN_TXT\n, NULL as SCALE_GRP_ID\n, INT(TASKITEM.SEQUENCE) as SEQ_NBR\n, NULL as SPL_PRCSG_CD\n, TRIM(TASKITEM.TASKITEMNAME) as TASK_ITM_NM\n, TRIM(TASKITEM.TASKTYPE) as TASK_TYPE_CD\n, TRIM(TASKITEM.TASKUSAGE) as TASK_USG_TXT\n, NULL as TLRNC_OVRD_ESIGN_ID\n, TRIM(TASKITEM.TRAININGREQGROUPID) as TRAIN_REQ_GRP_ID\n, TRIM(TASKITEM.TRANSACTIONPAGEID) as TRX_PG_ID\nFROM  {Config.sourceDatabase}.TASKITEM as TASKITEM  WHERE TASKITEM._deleted_ = 'F'\n  \n \n\"\"\")",
